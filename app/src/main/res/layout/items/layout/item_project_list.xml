<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="40dp"
    android:background="@color/white"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/item_project_list_xmmc"
        android:layout_width="@dimen/dp_75"
        android:layout_height="match_parent"
        style="@style/table_title_textview"
        android:text="项目名称"
        android:textSize="12sp"
        android:textColor="#333333"
        />
    <TextView
        android:id="@+id/item_project_list_xmlx"
        android:layout_width="@dimen/dp_75"
        android:layout_height="match_parent"
        style="@style/table_title_textview"
        android:text="项目类型"
        android:textSize="12sp"
        android:textColor="#333333"
        />
    <TextView
        android:id="@+id/item_project_list_ydgm"
        android:layout_width="@dimen/dp_75"
        android:layout_height="match_parent"
        style="@style/table_title_textview"
        android:text="用地规模"
        android:textSize="12sp"
        android:textColor="#333333"
        />
    <TextView
        android:id="@+id/item_project_list_jzgm"
        android:layout_width="@dimen/dp_75"
        android:layout_height="match_parent"
        style="@style/table_title_textview"
        android:text="建筑规模"
        android:textSize="12sp"
        android:textColor="#333333"
        />
    <TextView
        android:id="@+id/item_project_list_zjly"
        android:layout_width="@dimen/dp_75"
        android:layout_height="match_parent"
        style="@style/table_title_textview"
        android:text="资金来源"
        android:textSize="12sp"
        android:textColor="#333333"
        />
    <TextView
        android:visibility="gone"
        android:id="@+id/item_project_list_ztze"
        android:layout_width="@dimen/dp_75"
        android:layout_height="match_parent"
        style="@style/table_title_textview"
        android:text="总投资额"
        android:textSize="12sp"
        android:textColor="#333333"
        />
    <TextView
        android:visibility="gone"
        android:id="@+id/item_project_list_jz"
        android:layout_width="@dimen/dp_75"
        android:layout_height="match_parent"
        style="@style/table_title_textview"
        android:text="进展"
        android:textSize="12sp"
        android:textColor="#333333"
        />

</LinearLayout>