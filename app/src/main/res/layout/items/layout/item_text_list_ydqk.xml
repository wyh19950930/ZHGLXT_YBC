<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <LinearLayout
        android:minHeight="40dp"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            style="@style/table_title_textview"
            android:id="@+id/tv_qygl_item_list_xh"
            android:text="序号"
            android:layout_width="50dp"
            android:layout_height="match_parent"/>
        <TextView
            style="@style/table_title_textview"
            android:id="@+id/tv_qygl_item_list_tdsyr"
            android:text="土地使用人"
            android:layout_width="100dp"
            android:layout_height="match_parent"/>
        <TextView
            style="@style/table_title_textview"
            android:id="@+id/tv_qygl_item_list_tdxz"
            android:text="土地性质"
            android:layout_width="70dp"
            android:layout_height="match_parent"/>
        <TextView
            style="@style/table_title_textview"
            android:id="@+id/tv_qygl_item_list_qsqk"
            android:text="权属情况"
            android:layout_width="70dp"
            android:layout_height="match_parent"/>
        <TextView
            style="@style/table_title_textview"
            android:id="@+id/tv_qygl_item_list_tdqdnx"
            android:text="土地取得年限"
            android:layout_width="100dp"
            android:layout_height="match_parent"/>
        <TextView
            style="@style/table_title_textview"
            android:id="@+id/tv_qygl_item_list_tdsynx"
            android:text="土地剩余使用年限"
            android:layout_width="100dp"
            android:layout_height="match_parent"/>
        <TextView
            style="@style/table_title_textview"
            android:id="@+id/tv_qygl_item_list_tdjg"
            android:text="土地价格(万元/亩)"
            android:layout_width="80dp"
            android:layout_height="match_parent"/>
        <TextView
            style="@style/table_title_textview"
            android:id="@+id/tv_qygl_item_list_tdzmj"
            android:text="土地证面积(公顷)"
            android:layout_width="80dp"
            android:layout_height="match_parent"/>
        <TextView
            style="@style/table_title_textview"
            android:id="@+id/tv_qygl_item_list_tdqdfs"
            android:text="土地取得方式"
            android:layout_width="80dp"
            android:layout_height="match_parent"/>
        <TextView
            android:visibility="gone"
            android:textColor="#409EFF"
            style="@style/table_title_textview"
            android:id="@+id/tv_qygl_item_list_wjsc"
            android:text="上传"
            android:layout_width="50dp"
            android:layout_height="match_parent"/>
        <TextView
            android:textColor="#FF660F"
            style="@style/table_title_textview"
            android:id="@+id/tv_qygl_item_list_delete"
            android:text="删除"
            android:layout_width="50dp"
            android:layout_height="match_parent"/>
    </LinearLayout>

</RelativeLayout>