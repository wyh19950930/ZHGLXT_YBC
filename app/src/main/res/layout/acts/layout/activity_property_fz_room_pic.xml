                    android:layout_width="170dp"
                    android:layout_height="120dp">
                    <RelativeLayout
                        android:layout_width="150dp"
                        android:layout_height="100dp"
                        android:layout_centerInParent="true"
                        android:background="@drawable/icon_up_file">
