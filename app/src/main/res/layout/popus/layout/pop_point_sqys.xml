<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/rl_pop_point"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/popback"
    android:orientation="vertical"
    android:layout_margin="10dp">
    <RelativeLayout
        android:layout_gravity="center_horizontal"
        android:layout_width="350dp"
        android:layout_height="40dp">
        <TextView
            android:layout_centerInParent="true"
            android:textSize="16sp"
            android:textColor="#333333"
            android:text="延时"
            android:gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <ImageView
            android:src="@drawable/icon_zj_error"
            android:id="@+id/iv_pop_sqys_error"
            android:layout_marginRight="10dp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </RelativeLayout>
    <View
        android:background="#E0E0E0"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"/>
    <LinearLayout
        android:layout_marginRight="15dp"
        android:layout_marginLeft="15dp"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
<!--        案卷号-->
        <com.jymj.zhglxt.widget.zdybj.ZdyEditLayout
            app:attr_key="工单号"
            app:attr_hint="请输入"
            android:id="@+id/zel_pop_sqys_ajh"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
        <com.jymj.zhglxt.widget.zdybj.ZdyTextLayout
            app:attr_key="操作人"
            app:attr_hint="请输入"
            android:id="@+id/zel_pop_sqys_czr"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
        <com.jymj.zhglxt.widget.zdybj.ZdyEditLayout
            app:attr_key="延期天数"
            app:attr_hint="请输入"
            app:attr_isint="true"
            android:id="@+id/zel_pop_sqys_yqts"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
<!--        app:attr_arrows="@drawable/back_right_icon"-->
        <com.jymj.zhglxt.widget.zdybj.ZdyEditLayout
            app:attr_key="延期原因"
            app:attr_hint="请选择"
            android:id="@+id/zel_pop_qygl_yqyy"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
    </LinearLayout>
    <TextView
        android:id="@+id/tv_pop_sqys_sure"
        android:textColor="@color/white"
        android:background="@drawable/bt_actiive_409eff"
        android:layout_margin="10dp"
        android:gravity="center"
        android:layout_gravity="center_horizontal"
        android:text="确定"
        android:layout_width="66dp"
        android:layout_height="31dp"/>


</LinearLayout>