<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/rl_pop_point"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/popback"
    android:orientation="vertical"
    android:layout_margin="10dp">
    <RelativeLayout
        android:layout_gravity="center_horizontal"
        android:layout_width="350dp"
        android:layout_height="40dp">
        <TextView
            android:textColor="#333333"
            android:layout_centerInParent="true"
            android:textSize="16sp"
            android:text="经营情况"
            android:gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </RelativeLayout>
    <View
        android:background="@color/gray"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"/>
    <LinearLayout
        android:layout_marginRight="15.5dp"
        android:layout_marginLeft="15dp"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:layout_weight="1"
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_height="wrap_content">

                <com.jymj.zhglxt.widget.zdybj.ZdyEditLayout
                    app:attr_isfloat="true"
                    app:attr_key="利润总额"
                    app:attr_hint="请输入"
                    android:id="@+id/zel_pop_jyqk_lrze"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
                <com.jymj.zhglxt.widget.zdybj.ZdyEditLayout
                    app:attr_isfloat="true"
                    app:attr_key="税收"
                    app:attr_hint="请输入"
                    android:id="@+id/zel_pop_jyqk_ss"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
            </LinearLayout>
            <View
                android:layout_marginRight="5dp"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="10dp"
                android:background="#333333"
                android:layout_width="1dp"
                android:layout_height="match_parent"/>
            <LinearLayout
                android:layout_weight="1"
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_height="wrap_content">
<!--                app:attr_isfloat="true"-->
                <com.jymj.zhglxt.widget.zdybj.ZdyEditLayout

                    app:attr_key="营业收入"
                    app:attr_hint="请输入"
                    android:id="@+id/zel_pop_jyqk_yysr"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
                <com.jymj.zhglxt.widget.zdybj.ZdyEditLayout
                    app:attr_isfloat="true"
                    app:attr_key="产值"
                    app:attr_hint="请输入"
                    android:id="@+id/zel_pop_jyqk_cz"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
            </LinearLayout>
        </LinearLayout>
        <com.jymj.zhglxt.widget.zdybj.ZdyTextLayout
            app:attr_arrows="@drawable/back_right_icon"
            app:attr_key="经营状态"
            app:attr_hint="请选择"
            android:id="@+id/zel_pop_jyqk_ywdxkj"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <com.jymj.zhglxt.widget.zdybj.ZdyEditLayout
            app:attr_isint="true"
            app:attr_key="本地员工数量"
            app:attr_hint="请输入"
            android:id="@+id/zel_pop_jyqk_bdygsl"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
        <com.jymj.zhglxt.widget.zdybj.ZdyEditLayout
            app:attr_isint="true"
            app:attr_key="外地员工数量"
            app:attr_hint="请输入"
            android:id="@+id/zel_pop_jyqk_wdygsl"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

        <com.jymj.zhglxt.widget.zdybj.ZdyEditLayout
            app:attr_isint="true"
            app:attr_key="就业人口居住区域(单位宿舍)"
            app:attr_hint="请输入"
            android:id="@+id/zel_pop_jyqk_jyrkjzqy1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

        <com.jymj.zhglxt.widget.zdybj.ZdyEditLayout
            app:attr_isint="true"
            app:attr_key="就业人口居住区域(本村)"
            app:attr_hint="请输入"
            android:id="@+id/zel_pop_jyqk_jyrkjzqy2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

        <com.jymj.zhglxt.widget.zdybj.ZdyEditLayout
            app:attr_isint="true"
            app:attr_key="就业人口居住区域(其他)"
            app:attr_hint="请输入"
            android:id="@+id/zel_pop_jyqk_jyrkjzqy3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:layout_weight="1"
            android:textColor="#4CA2FE"
            android:id="@+id/but_pop_jyqk_sure"
            android:layout_gravity="center_horizontal"
            android:gravity="center"
            android:text="确定"
            android:layout_width="0dp"
            android:layout_height="35dp"/>
        <View
            android:background="#E0E0E0"
            android:layout_marginBottom="11dp"
            android:layout_marginTop="8dp"
            android:layout_width="0.5dp"
            android:layout_height="match_parent"/>
        <TextView
            android:layout_weight="1"
            android:textColor="#999999"
            android:id="@+id/but_pop_jyqk_close"
            android:layout_gravity="center_horizontal"
            android:gravity="center"
            android:text="取消"
            android:layout_width="0dp"
            android:layout_height="35dp"/>

    </LinearLayout>

</LinearLayout>