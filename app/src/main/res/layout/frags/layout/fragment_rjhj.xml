                    android:id="@+id/ll_frag_rjhj_list_tab"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:orientation="horizontal"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="5dp">
                    <RelativeLayout
                        android:id="@+id/rl_frag_rjhj_wxf"
                        android:layout_width="90dp"
                        android:layout_height="match_parent"
                        android:background="@drawable/rjhj_zg_rlt"
                        android:layout_margin="5dp">
                        <TextView
                            android:id="@+id/tv_frag_rjhj_wxf_tab"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="未下发"
                            android:textColor="@color/white"
                            android:textSize="8sp"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="2.5dp"
                            />
                        <View
                            android:id="@+id/view_frag_rjhj_wxf_tab"
                            android:layout_width="15dp"
                            android:layout_height="2.5dp"
                            android:background="@drawable/mapback_ffffff"
                            android:layout_below="@+id/tv_frag_rjhj_wxf_tab"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="1dp"/>
                        <TextView
                            android:id="@+id/tv_frag_rjhj_wxf"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="158"
                            android:textSize="18sp"
                            android:textColor="@color/white"
                            android:layout_alignParentBottom="true"
                            android:layout_alignParentRight="true"
                            android:layout_marginRight="9dp"
                            android:layout_marginBottom="1.5dp"
                            />
                    </RelativeLayout>
                    <RelativeLayout
                        android:id="@+id/rl_frag_rjhj_xc"
                        android:layout_width="90dp"
                        android:layout_height="match_parent"
                        android:background="@drawable/rjhj_xc_rlt"
                        android:layout_margin="5dp"
                        >
                        <TextView
                            android:id="@+id/tv_frag_rjhj_xc_tab"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="巡查"
                            android:textColor="@color/white"
                            android:textSize="8sp"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="2.5dp"
                            />
                        <View
                            android:id="@+id/view_frag_rjhj_xc_tab"
                            android:layout_width="15dp"
                            android:layout_height="2.5dp"
                            android:background="@drawable/mapback_ffffff"
                            android:layout_below="@+id/tv_frag_rjhj_xc_tab"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="1dp"/>
                        <TextView
                            android:id="@+id/tv_frag_rjhj_xc"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="158"
                            android:textSize="18sp"
                            android:textColor="@color/white"
                            android:layout_alignParentBottom="true"
                            android:layout_alignParentRight="true"
                            android:layout_marginRight="9dp"
                            android:layout_marginBottom="1.5dp"
                            />
                    </RelativeLayout>
                    <RelativeLayout
                        android:id="@+id/rl_frag_rjhj_zg"
                        android:layout_width="90dp"
                        android:layout_height="match_parent"
                        android:background="@drawable/rjhj_zg_rlt"
                        android:layout_margin="5dp">
                        <TextView
                            android:id="@+id/tv_frag_rjhj_zg_tab"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="整改"
                            android:textColor="@color/white"
                            android:textSize="8sp"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="2.5dp"
                            />
                        <View
                            android:visibility="gone"
                            android:id="@+id/view_frag_rjhj_zg_tab"
                            android:layout_width="15dp"
                            android:layout_height="2.5dp"
                            android:background="@drawable/mapback_ffffff"
                            android:layout_below="@+id/tv_frag_rjhj_zg_tab"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="1dp"/>
                        <TextView
                            android:id="@+id/tv_frag_rjhj_zg"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="158"
                            android:textSize="18sp"
                            android:textColor="@color/white"
                            android:layout_alignParentBottom="true"
                            android:layout_alignParentRight="true"
                            android:layout_marginRight="9dp"
                            android:layout_marginBottom="1.5dp"
                            />
                    </RelativeLayout>
                    <RelativeLayout
                        android:id="@+id/rl_frag_rjhj_sh"
                        android:layout_width="90dp"
                        android:layout_height="match_parent"
                        android:background="@drawable/rjhj_sh_rlt"
                        android:layout_margin="5dp">
                        <TextView
                            android:id="@+id/tv_frag_rjhj_sh_tab"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="审核"
                            android:textColor="@color/white"
                            android:textSize="8sp"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="2.5dp"
                            />
                        <View
                            android:visibility="gone"
                            android:id="@+id/view_frag_rjhj_sh_tab"
                            android:layout_width="15dp"
                            android:layout_height="2.5dp"
                            android:background="@drawable/mapback_ffffff"
                            android:layout_below="@+id/tv_frag_rjhj_sh_tab"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="1dp"/>
                        <TextView
                            android:id="@+id/tv_frag_rjhj_sh"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="158"
                            android:textSize="18sp"
                            android:textColor="@color/white"
                            android:layout_alignParentBottom="true"
                            android:layout_alignParentRight="true"
                            android:layout_marginRight="9dp"
                            android:layout_marginBottom="1.5dp"
                            />
                    </RelativeLayout>
                    <RelativeLayout
                        android:id="@+id/rl_frag_rjhj_xz"
                        android:layout_width="90dp"
                        android:layout_height="match_parent"
                        android:background="@drawable/rjhj_xz_rlt"
                        android:layout_margin="5dp">
                        <TextView
                            android:id="@+id/tv_frag_rjhj_xz_tab"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="销账"
