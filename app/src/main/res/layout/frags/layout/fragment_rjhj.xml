                android:visibility="gone"
                android:id="@+id/tv_frag_rjhj_add"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                android:textSize="36sp"
                android:textColor="@color/red"
                android:text="+"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <RelativeLayout
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:layout_marginLeft="14dp"
                android:layout_marginBottom="10dp"
                android:background="@drawable/mapback_ffffff"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent">

                <ImageView
                    android:id="@+id/iv_frag_rjhj_location"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_centerInParent="true"
                    android:src="@drawable/login_home" />
            </RelativeLayout>

            <LinearLayout
                android:id="@+id/ll_frag_rjhj_but"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="50dp"
                android:layout_marginRight="12dp"
                android:background="@drawable/mapback_ffffff"
                android:orientation="vertical"
                android:padding="5dp"
                app:layout_constraintEnd_toEndOf="parent"
                sothree:layout_constraintTop_toTopOf="parent">
                <LinearLayout
                    android:id="@+id/ll_frag_rjhj_point"
                    android:layout_width="40dp"
                    android:layout_height="45dp"
                    android:layout_marginBottom="5dp"
                    android:background="@drawable/mapback_ffffff"
                    android:orientation="vertical"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <ImageView
                        android:layout_margin="3dp"
                        android:layout_weight="1"
                        android:id="@+id/iv_frag_rjhj_point"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="3dp"
                        android:src="@drawable/rjhj_add_wt" />

                    <TextView
                        android:textColor="#333333"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:text="点查"
                        android:textSize="12.5sp" />
                </LinearLayout>

                <View
                    android:visibility="gone"
                    android:id="@+id/view_frag_rjhj_point_clear"
                    android:layout_width="25dp"
                    android:layout_height="0.5dp"
                    android:layout_gravity="center"
                    android:background="#e0e0e0" />

                <LinearLayout
                    android:id="@+id/ll_frag_rjhj_clear"
                    android:layout_width="40dp"
                    android:layout_height="45dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/mapback_ffffff"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/iv_frag_rjhj_clear"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_centerInParent="true"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="3dp"
                        android:src="@drawable/clear_icon" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:text="点查"
                        android:textSize="10sp"
                        android:visibility="gone" />
                </LinearLayout>

            </LinearLayout>
            <LinearLayout
                android:visibility="gone"
                android:id="@+id/ll_frag_gddw_but"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="50dp"
                android:layout_marginRight="12dp"
                android:background="@drawable/mapback_ffffff"
                android:orientation="vertical"
                android:padding="5dp"
                app:layout_constraintEnd_toEndOf="parent"
                sothree:layout_constraintTop_toTopOf="parent">

                <LinearLayout
                    android:id="@+id/ll_frag_gddw_add"
                    android:layout_width="40dp"
                    android:layout_height="45dp"
                    android:layout_marginBottom="5dp"
                    android:background="@drawable/mapback_ffffff"
                    android:orientation="vertical"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <ImageButton
                        android:background="@color/white"
                        android:id="@+id/iv_frag_gddw_add"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_centerInParent="true"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="3dp"
                        android:src="@drawable/button_selector_active_src_gddw_add" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:text="点查"
                        android:textSize="10sp"
                        android:visibility="gone" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll_frag_gddw_update"
                    android:layout_width="40dp"
                    android:layout_height="45dp"
                    android:layout_marginBottom="5dp"
                    android:background="@drawable/mapback_ffffff"
                    android:orientation="vertical"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <ImageButton
                        android:background="@color/white"
                        android:id="@+id/iv_frag_gddw_update"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_centerInParent="true"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="3dp"
                        android:src="@drawable/button_selector_active_src_gddw_update" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:text="点查"
                        android:textSize="10sp"
                        android:visibility="gone" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_frag_gddw_clear"
                    android:layout_width="40dp"
                    android:layout_height="45dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/mapback_ffffff"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/iv_frag_gddw_clear"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_centerInParent="true"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="3dp"
                        android:src="@drawable/clear_icon" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:text="点查"
                        android:textSize="10sp"
                        android:visibility="gone" />
                </LinearLayout>

            </LinearLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>
    </LinearLayout>
    <ScrollView
        android:id="@+id/sv_frag_rjhj_up"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <LinearLayout
