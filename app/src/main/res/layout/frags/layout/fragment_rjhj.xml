                            android:id="@+id/cet_frag_rjhj_cm_lgy"
                            android:layout_marginLeft="5dp"
                            android:background="@null"
                            android:hint="请输入编号"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:textColor="#999999"
                            android:textSize="14sp"
                            android:maxLength="9"
                            android:inputType="phone|number"
                            android:digits="1234567890"
                            />

                    </LinearLayout>
                    <TextView
                        android:id="@+id/tv_act_czlb_search"
                        android:padding="11dp"
                        android:textColor="@color/white"
                        android:text="搜索"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:id="@+id/ll_frag_rjhj_czmc"
                        android:layout_weight="1"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:gravity="center">
                        <TextView
                            android:id="@+id/tv_frag_rjhj_czmc"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:text="村庄名称"
                            android:textColor="#666666"
                            android:textSize="14sp"/>
                        <ImageView
                            android:layout_width="15dp"
                            android:layout_height="15dp"
                            android:layout_gravity="center"
                            android:layout_marginLeft="3dp"
                            android:layout_marginTop="3dp"
                            android:background="@drawable/inverted_triangle_style_grey" />
                    </LinearLayout>
                    <View
                        android:layout_width="1dp"
                        android:layout_height="match_parent"
                        android:background="#cccccc"/>
                    <LinearLayout
                        android:id="@+id/ll_frag_rjhj_wtlx"
                        android:layout_weight="1"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:gravity="center">
                        <TextView
                            android:id="@+id/tv_frag_rjhj_wtlx"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:text="问题类型"
                            android:textColor="#666666"
                            android:textSize="14sp"/>
                        <ImageView
                            android:layout_width="15dp"
                            android:layout_height="15dp"
                            android:layout_gravity="center"
                            android:layout_marginLeft="3dp"
                            android:layout_marginTop="3dp"
                            android:background="@drawable/inverted_triangle_style_grey" />
                    </LinearLayout>
                    <View
                        android:layout_width="1dp"
                        android:layout_height="match_parent"
                        android:background="#cccccc"/>
                    <LinearLayout
                        android:id="@+id/ll_frag_rjhj_zdwt"
                        android:layout_weight="1"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:gravity="center">
                        <TextView
                            android:id="@+id/tv_frag_rjhj_zdwt"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:gravity="center"
                            android:text="重大问题"
                            android:textColor="#666666"
                            android:textSize="14sp"/>
                        <ImageView
                            android:layout_width="15dp"
                            android:layout_height="15dp"
                            android:layout_gravity="center"
                            android:layout_marginLeft="3dp"
                            android:layout_marginTop="3dp"
                            android:background="@drawable/inverted_triangle_style_grey" />
                    </LinearLayout>
                    <View
                        android:layout_width="1dp"
                        android:layout_height="match_parent"
                        android:background="#cccccc"/>

                    <ImageView
                        android:id="@+id/iv_frag_rjhj_reset"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/reset_option_icon"
                        android:layout_gravity="center"
                        android:padding="11dp"/>

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="5dp"
                    android:background="#F6F7FB"
                    />

                <LinearLayout
