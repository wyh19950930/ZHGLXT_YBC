<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <LinearLayout
        android:padding="7dp"
        android:background="@drawable/bt_actiive_xaiyoubai_white"
        android:layout_marginRight="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginBottom="5dp"
        android:layout_marginTop="5dp"
        android:minHeight="80dp"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:drawablePadding="5dp"
                android:textColor="@color/black"
                android:text="农户家庭信息"
                android:drawableLeft="@drawable/icon_nhjtxx"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
<!--     未提交       bt_actiive_xaiyoubai_orange-->
<!--            #F98648-->
            <TextView
                android:id="@+id/tv_item_bcjc_nyktkcb_issum"
                android:gravity="center"
                android:text="已提交"
                android:layout_marginLeft="10dp"
                android:textColor="#20B759"
                android:background="@drawable/bt_actiive_xaiyoubai_green"
                android:layout_width="60dp"
                android:layout_height="25dp"/>
            <View
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="1dp"/>
            <TextView
                android:id="@+id/tv_item_bcjc_nyktkcb_wjbh"
                android:layout_marginRight="5dp"
                android:textColor="#20B759"
                android:text="问卷编号：5211345"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>


        </LinearLayout>
        <LinearLayout
            android:layout_marginTop="15dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/tv_bcjc_nyjtjcb_xh"
                android:layout_marginLeft="25dp"
                android:layout_gravity="center_vertical"
                android:textStyle="bold"
                android:text="01"
                android:textSize="35dp"
                android:textColor="#666666"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <View
                android:background="@color/gray"
                android:layout_marginRight="7.5dp"
                android:layout_marginLeft="7.5dp"
                android:layout_width="1dp"
                android:layout_height="match_parent"/>
            <LinearLayout
                android:gravity="center_vertical"
                android:layout_weight="1"
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="match_parent">
                <TextView
                    android:id="@+id/tv_item_bcjc_nyktkcb_mph"
                    android:textStyle="bold"
                    android:textColor="#333333"
                    android:textSize="16sp"
                    android:text="北臧村32451号"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
                <TextView
                    android:id="@+id/tv_item_bcjc_nyktkcb_time"
                    android:textSize="11sp"
                    android:textColor="#666666"
                    android:text="2021年4月16日"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>

            </LinearLayout>
<!--            bt_actiive_shi_blue-->
            <TextView
                android:layout_gravity="center_vertical"
                android:background="@drawable/bt_actiive_kong_blue"
                android:gravity="center"
                android:textColor="#409EFF"
                android:text="点击查看"
                android:layout_width="80dp"
                android:layout_height="30dp"/>

        </LinearLayout>

    </LinearLayout>

