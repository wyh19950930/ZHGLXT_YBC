<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.jymj.zhglxt.ui.fragment.HomePageFragment">

    <RelativeLayout
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="50dp">
<!--        xx模块-->
        <TextView
            android:id="@+id/tv_frag_homepage_title"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:gravity="center"
            android:text=""
            android:textColor="@color/black"
            android:textSize="18.5sp"
            android:transitionName="title" />

        <TextView
            android:drawableLeft="@drawable/xzc"
            android:textSize="16sp"
            android:drawablePadding="5dp"
            android:drawableRight="@drawable/back_down_icon"
            android:id="@+id/tv_frag_homepage_left"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginLeft="5dp"
            android:gravity="center_vertical"
            android:text="请选择" />

        <TextView
            android:gravity="center_vertical"
            android:visibility="gone"
            android:textColor="@color/blue"
            android:id="@+id/tv_frag_homepage_left_back"
            android:layout_marginLeft="5dp"
            android:layout_centerVertical="true"
            android:text="返回"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"/>

        <ImageView
            android:visibility="gone"
            android:id="@+id/iv_frag_homepage_left"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:src="@drawable/qrcode_home"
            android:padding="15dp"/>


        <ImageView
            android:padding="13dp"
            android:id="@+id/iv_frag_homepage_right"
            android:src="@drawable/tc_kong"
            android:layout_alignParentRight="true"
            android:layout_width="50dp"
            android:layout_height="50dp" />
    </RelativeLayout>
    <FrameLayout
        android:id="@+id/flt_homepage"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="50dp">

    </FrameLayout>
    <androidx.recyclerview.widget.RecyclerView
        android:visibility="gone"
        android:id="@+id/recy_map_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/mapback_ffffff" />

    <RelativeLayout
        android:visibility="gone"
        android:id="@+id/rl_all_icon_container"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="160dp">
        <RelativeLayout
            android:id="@+id/rl_homepage_layer"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="12dp"
            android:background="@drawable/mapback"
            android:transitionName="layer">

            <ImageButton
                android:id="@+id/bt_homepage_layer"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:background="@drawable/layers_icon"
                android:scaleType="fitCenter" />
<!--            button_selector_active_src_layers-->
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/rl_homepage_zjd"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="12dp"
            android:layout_below="@+id/rl_homepage_layer"
            android:background="@drawable/mapback"
            android:transitionName="layer">

            <ImageButton

                android:id="@+id/bt_homepage_zjd"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_centerInParent="true"
                android:background="@drawable/button_selector"
                android:scaleType="fitCenter"
                android:src="@drawable/button_selector_active_src_zjd" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_homepage_cy"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_below="@+id/rl_homepage_zjd"
            android:layout_marginStart="12dp"
            android:layout_marginTop="12dp"
            android:background="@drawable/mapback"
            android:transitionName="layer">

            <ImageButton
                android:id="@+id/bt_homepage_cy"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_centerInParent="true"
                android:background="@drawable/button_selector"
                android:scaleType="fitCenter"
                android:src="@drawable/button_selector_active_src_qy" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_homepage_yd"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_below="@+id/rl_homepage_cy"
            android:layout_marginStart="12dp"
            android:layout_marginTop="12dp"
            android:background="@drawable/mapback"
            android:transitionName="layer">

            <ImageButton
                android:id="@+id/bt_homepage_yd"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_centerInParent="true"
                android:background="@drawable/button_selector"
                android:scaleType="fitCenter"
                android:src="@drawable/button_selector_active_src_yd" />
        </RelativeLayout>
    </RelativeLayout>





</RelativeLayout>
