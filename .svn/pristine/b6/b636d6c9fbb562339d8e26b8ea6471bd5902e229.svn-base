<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:id="@+id/ll_act_ajlb"
    tools:context="com.jymj.zhglxt.zjd.activity.AjlbActivity">
    <RelativeLayout
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="50dp">
        <ImageView
            android:padding="12dp"
            android:id="@+id/ic_act_ajlb_back"
            android:layout_centerVertical="true"
            android:src="@drawable/back_left_icon"
            android:layout_width="50dp"
            android:layout_height="50dp"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="案件列表"
            android:textColor="@color/black"
            android:textSize="18sp"
            android:textStyle="bold" />
        <ImageView
            android:visibility="gone"
            android:layout_marginRight="15dp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:src="@drawable/search_home"
            android:id="@+id/ic_act_ajlb_search"
            android:layout_width="18dp"
            android:layout_height="18dp"/>

    </RelativeLayout>
    <LinearLayout
        android:gravity="center_vertical"
        android:background="#4CA2FE"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="42dp">
        <LinearLayout
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:gravity="center_vertical"
            android:background="@drawable/mapback_ffffff2"
            android:layout_weight="1"
            android:layout_marginLeft="15dp"
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="30dp">
            <ImageView
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="12dp"
                android:src="@drawable/hssearch_czlb"
                android:layout_width="15dp"
                android:layout_height="15dp"/>
            <com.jymj.zhglxt.widget.ClearEditText
                android:layout_marginRight="10dp"
                android:id="@+id/cet_act_ajlb_search_title"
                android:layout_marginLeft="5dp"
                android:background="@null"
                android:hint="请输入标题"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textColor="#999999"
                android:textSize="14sp"
                />
            <!--android:maxLength="9"
            android:inputType="phone|number"
            android:digits="1234567890"-->

        </LinearLayout>
        <TextView
            android:id="@+id/tv_act_ajlb_search_title"
            android:padding="11dp"
            android:textColor="@color/white"
            android:text="搜索"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </LinearLayout>
    <LinearLayout
        android:layout_marginRight="12dp"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/mapback_ffffff"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:gravity="center_vertical"
            android:layout_weight="1"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="38.5dp">
            <View
                android:visibility="gone"
                android:background="#4CA2FE"
                android:layout_width="5dp"
                android:layout_height="15dp"/>
            <TextView
                android:layout_marginLeft="15.5dp"
                android:textSize="16sp"
                android:textColor="#333333"
                android:text="案件列表"
                android:gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"/>
            <View
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"/>
            <!--        android:drawableLeft="@drawable/icon_jtcytj"
            android:text="添加"-->
            <TextView
                android:textColor="#999999"
                android:text="指示灯"
                android:gravity="center_vertical"
                android:id="@+id/tv_act_ajlb_zsd"
                android:drawablePadding="5dp"
                android:drawableRight="@drawable/icon_zsd"
                android:layout_marginRight="7dp"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"/>
        </LinearLayout>
        <View
            android:background="#333333"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"/>
        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rlv_act_ajlb"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
    </LinearLayout>
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <!--<com.jymj.zhglxt.widget.zdybj.MeTitleLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:attr_color="#4CA2FE"
            app:attr_name="案件列表"
            app:attr_isshow_right="false"
            />-->

    </LinearLayout>

</LinearLayout>