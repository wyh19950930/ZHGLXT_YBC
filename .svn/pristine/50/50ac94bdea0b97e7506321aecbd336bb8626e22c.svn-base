ext {
    android = [
            compileSdkVersion: 27,
            buildToolsVersion: '28.0.0 rc2',//28.0.0 rc2
            applicationId    : 'com.jymj.projectmanager',
            minSdkVersion    : 21,
            targetSdkVersion : 28,
            versionCode      : 10,
            versionName      : '0.1.0',
            flavorDimensions : 'versionCode',

    ]
//
    dependencies = [
            "appcompat-v7"                 : "com.android.support:appcompat-v7:$support_version",
            "support-design"               : "com.android.support:design:$support_version",
            "support-v4"                   : "com.android.support:support-v4:$support_version",
            "support-annotations"          : "com.android.support:support-annotations:$support_version",
            "kotlin"                       : "org.jetbrains.kotlin:kotlin-stdlib-jdk7:$kotlin_version",
            "multidex"                     : 'com.android.support:multidex:1.0.3',//com.android.support:multidex:1.0.3
            "okgo"                         : 'com.lzy.net:okgo:3.0.4',
            "glide"                        : "com.github.bumptech.glide:glide:$glide_version",
            "glide-compiler"               : "com.github.bumptech.glide:compiler:$glide_version",
            "gson"                         : 'com.google.code.gson:gson:2.8.2',
            "logger"                       : 'com.orhanobut:logger:2.1.1',
            "chuck"                        : 'com.readystatesoftware.chuck:library:1.0.4',

            "junit"                        : 'junit:junit:4.12',
            "runner"                       : 'com.android.support.test:runner:1.0.1',
            "espresso"                     : 'com.android.support.test.espresso:espresso-core:3.0.1',
            "MPAndroidChart"               : 'com.github.PhilJay:MPAndroidChart:v3.0.3',
            "amap-map2d"                   : 'com.amap.api:map2d:5.2.0',
            "amap-location"                : 'com.amap.api:location:4.1.0',
            "greendao"                     : 'org.greenrobot:greendao:3.2.2',
            "BaseRecyclerViewAdapterHelper": 'com.github.CymChad:BaseRecyclerViewAdapterHelper:2' +
                    '.9.40',
            "constraint-layout"            : 'com.android.support.constraint:constraint-layout:1.1.2',
            "android-process-button"       : 'com.github.dmytrodanylyk.android-process-button:library:1.0.4',
            "photoview"                    : 'cn.bluemobi.dylan:photoview:1.3',
            "sweetalert"                   : 'com.github.f0ris.sweetalert:library:1.5.1',
            "alicloud-android-hotfix"      : 'com.aliyun.ams:alicloud-android-hotfix:3.2.3',//3.2.3
            "rxandroid"                    : 'io.reactivex.rxjava2:rxandroid:2.0.2',
            "rxjava"                       : 'io.reactivex.rxjava2:rxjava:2.1.10',
            "rxkotlin"                     : 'io.reactivex.rxjava2:rxkotlin:2.2.0',
            "rxpermissions"                : 'com.tbruyelle.rxpermissions2:rxpermissions:0.9.5@aar',
            "cardview"                     : "com.android.support:cardview-v7:$support_version"
    ]
}