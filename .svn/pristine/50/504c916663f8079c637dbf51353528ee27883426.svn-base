package com.jymj.zhglxt.zjd.bean;

import java.math.BigDecimal;

/**
 * @Author: Mr.haozi
 * @CreateDate: 2020/4/9 13:42
 */
public class CJBarChartBean {
    private BigDecimal zdmj;
    private BigDecimal jzmj;
    private String name;

    public BigDecimal getZdmj() {
        return zdmj==null?new BigDecimal(0):zdmj;
    }

    public void setZdmj(BigDecimal zdmj) {
        this.zdmj = zdmj;
    }

    public BigDecimal getJzmj() {
        return jzmj==null?new BigDecimal(0):jzmj;
    }

    public void setJzmj(BigDecimal jzmj) {
        this.jzmj = jzmj;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public CJBarChartBean(BigDecimal zdmj, BigDecimal jzmj, String name) {
        this.zdmj = zdmj;
        this.jzmj = jzmj;
        this.name = name;
    }
}
