<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical"
    android:id="@+id/ll_frag_zjd_wjdc"
    tools:context="com.jymj.zhglxt.zjd.fragment.ZjdYdFragment">


    <ScrollView
        android:id="@+id/slv_frag_zjd_wjdc"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">
                <View
                    android:layout_width="3dp"
                    android:layout_height="match_parent"
                    android:layout_margin="10dp"
                    android:background="#409EFF" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="人口变动情况"
                    android:textColor="@color/black"
                    android:textSize="16sp"
                    android:textStyle="bold" />

                <com.jymj.zhglxt.widget.zdybj.ZdyTextLayout_1
                    android:id="@+id/tv_frag_zjd_wjdc"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="30dp"
                    app:attr_hint="请选择时间"
                    app:attr_key="村集体经济运行情况监测问卷" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/gray" />
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="40dp">
                <TextView
                    android:gravity="center"
                    android:layout_marginTop="10dp"
                    android:textStyle="bold"
                    android:textSize="15sp"
                    android:textColor="@color/black"
                    android:text="村庄未来集体产业发展思路及政策建议"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
                <TextView
                    android:visibility="gone"
                    android:gravity="center_vertical"
                    android:layout_marginRight="10dp"
                    android:layout_alignParentRight="true"
                    android:id="@+id/tv_frag_zjd_wjdc_update"
                    android:textSize="15sp"
                    android:textColor="#4CA2FE"
                    android:textStyle="bold"
                    android:text="操作"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"/>
            </RelativeLayout>
            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <LinearLayout
                        android:minHeight="40dp"
                        android:gravity="center_horizontal"
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <LinearLayout
                            android:gravity="center_vertical"
                            android:id="@+id/ll_frag_zjd_wjdc_xz"
                            android:orientation="horizontal"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="乡镇:"
                                android:textColor="@color/black" />
                            <TextView
                                android:textColor="@color/black"
                                android:textSize="15sp"
                                android:gravity="center_vertical"
                                android:id="@+id/et_frag_zjd_wjdc_xz"
                                android:minWidth="60dp"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"/>
                        </LinearLayout>
                        <LinearLayout
                            android:gravity="center_vertical"
                            android:id="@+id/ll_frag_zjd_wjdc_cun"
                            android:orientation="horizontal"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent">
                            <TextView
                                android:textColor="@color/black"
                                android:text="村:"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>
                            <TextView
                                android:textColor="@color/black"
                                android:textSize="15sp"
                                android:gravity="center_vertical"
                                android:id="@+id/et_frag_zjd_wjdc_cun"
                                android:minWidth="60dp"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"/>
                        </LinearLayout>
                        <LinearLayout
                            android:gravity="center_vertical"
                            android:id="@+id/ll_frag_zjd_wjdc_tbr"
                            android:orientation="horizontal"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent">
                            <TextView
                                android:textColor="@color/black"
                                android:text="填表人:"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>
                            <EditText
                                android:id="@+id/et_frag_zjd_wjdc_tbr"
                                android:minWidth="60dp"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"/>
                        </LinearLayout>
                        <LinearLayout
                            android:gravity="center_vertical"
                            android:id="@+id/ll_frag_zjd_wjdc_fhr"
                            android:orientation="horizontal"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent">
                            <TextView
                                android:textColor="@color/black"
                                android:text="复核人:"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>
                            <EditText
                                android:id="@+id/et_frag_zjd_wjdc_fhr"
                                android:minWidth="60dp"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"/>
                        </LinearLayout>

                    </LinearLayout>
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_marginTop="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <TextView
                            android:id="@+id/ev_frag_zjd_wjdc_zyzy"
                            android:textSize="15sp"
                            android:textColor="#3E3E3E"
                            android:text="1.目前，本村集体产业发展的主要制约因素是:"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>
                        <TextView
                            android:text="(注：按重要性排序多选，不超过3个)"
                            android:layout_marginTop="5dp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>
                    </LinearLayout>
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/rlv_frag_zjd_wjdc_zyys"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"/>

                    </RelativeLayout>
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_marginTop="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:id="@+id/tv_frag_zjd_wjdc_wljhfz"
                            android:textSize="15sp"
                            android:textColor="#3E3E3E"
                            android:text="2.村庄未来计划发展的产业是:"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>
                        <TextView
                            android:text="(注：可多选)"
                            android:layout_marginTop="5dp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>
                    </LinearLayout>
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/rlv_frag_zjd_wjdc_wljhfz"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"/>
                    </RelativeLayout>
                    <LinearLayout
                        android:id="@+id/ll_frag_zjd_wjdc_cyfz"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_marginTop="10dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <TextView
                                android:id="@+id/tv_frag_zjd_wjdc_nfmzc"
                                android:textSize="15sp"
                                android:textColor="#3E3E3E"
                                android:text="3.村庄产业发展，最需要哪些方面的支持:"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>
                            <TextView
                                android:text="(注：按重要性排序多选，不超过3个)"
                                android:layout_marginTop="5dp"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>
                        </LinearLayout>
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/rlv_frag_zjd_wjdc_nfmzc"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"/>
                                    </RelativeLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="20dp"
                        android:gravity="center"
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <Button
                            android:visibility="gone"
                            android:id="@+id/but_frag_zjd_wjdc_save"
                            android:layout_marginRight="13.5dp"
                            android:layout_marginLeft="16dp"
                            android:background="@drawable/mapback_7fdfbb"
                            android:textColor="@color/white"
                            android:text="保存"
                            android:layout_width="wrap_content"
                            android:layout_height="39dp"/>
                        <Button
                            android:visibility="gone"
                            android:id="@+id/but_frag_zjd_wjdc_tj"
                            android:layout_marginRight="13.5dp"
                            android:layout_marginLeft="16dp"
                            android:background="@drawable/mapback_449eef"
                            android:textColor="@color/white"
                            android:text="提交"
                            android:layout_width="wrap_content"
                            android:layout_height="39dp"/>

                    </LinearLayout>
                </LinearLayout>
                <TextView
                    android:id="@+id/tv_frag_zjd_wjdc_isupdate"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
            </FrameLayout>


        </LinearLayout>
    </ScrollView>

</LinearLayout>