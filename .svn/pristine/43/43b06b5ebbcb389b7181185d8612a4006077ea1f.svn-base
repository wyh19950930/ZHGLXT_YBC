<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ll_act_rjhj_dwck_top"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical"
    tools:context=".rjhj.activity.RjhjDwckActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="44dp">

        <ImageView
            android:id="@+id/tv_act_rjhj_dwck_back"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_centerVertical="true"
            android:paddingLeft="15dp"
            android:paddingTop="11dp"
            android:paddingRight="15dp"
            android:paddingBottom="11dp"
            android:src="@drawable/back_left_icon" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="点位查看"
            android:textColor="@color/black"
            android:textSize="18sp" />

    </RelativeLayout>

    <LinearLayout
        android:layout_weight="1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="#333333" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:minHeight="30dp"
                    android:layout_marginLeft="14.5dp"
                    android:layout_marginRight="14.5dp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="名称"
                        android:textColor="#999999"
                        android:textSize="14sp" />

                    <TextView
                        android:id="@+id/tv_act_rjhj_dwck_name"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="right"
                        android:text="公厕管护"
                        android:textColor="#333333"
                        android:textSize="14sp" />

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginLeft="14.5dp"
                    android:layout_marginRight="14.5dp"
                    android:background="#999999" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:minHeight="30dp"
                    android:layout_marginLeft="14.5dp"
                    android:layout_marginRight="14.5dp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="乡镇"
                        android:textColor="#999999"
                        android:textSize="14sp" />

                    <TextView
                        android:id="@+id/tv_act_rjhj_dwck_xz"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="right"
                        android:text="xx镇"
                        android:textColor="#333333"
                        android:textSize="14sp" />

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginLeft="14.5dp"
                    android:layout_marginRight="14.5dp"
                    android:background="#999999" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:minHeight="30dp"
                    android:layout_marginLeft="14.5dp"
                    android:layout_marginRight="14.5dp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="村"
                        android:textColor="#999999"
                        android:textSize="14sp" />

                    <TextView
                        android:id="@+id/tv_act_rjhj_dwck_cun"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="right"
                        android:text="xx村"
                        android:textColor="#333333"
                        android:textSize="14sp" />

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginLeft="14.5dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginRight="14.5dp"
                    android:background="#999999" />

            </LinearLayout>

            <View
                android:layout_width="0.5dp"
                android:layout_height="100dp"
                android:layout_marginTop="9.5dp"
                android:background="#333333" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:minHeight="30dp"
                    android:layout_marginLeft="14.5dp"
                    android:layout_marginRight="14.5dp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="编号"
                        android:textColor="#999999"
                        android:textSize="14sp" />

                    <TextView
                        android:id="@+id/tv_act_rjhj_dwck_bh"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="right"
                        android:text="xx"
                        android:textColor="#333333"
                        android:textSize="14sp" />

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginLeft="14.5dp"
                    android:layout_marginRight="14.5dp"
                    android:background="#999999" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:minHeight="30dp"
                    android:layout_marginLeft="14.5dp"
                    android:layout_marginRight="14.5dp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="地址"
                        android:textColor="#999999"
                        android:textSize="14sp" />

                    <TextView
                        android:id="@+id/tv_act_rjhj_dwck_address"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="right"
                        android:text="xx"
                        android:textColor="#333333"
                        android:textSize="14sp" />

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginLeft="14.5dp"
                    android:layout_marginRight="14.5dp"
                    android:background="#999999" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:minHeight="30dp"
                    android:layout_marginLeft="14.5dp"
                    android:layout_marginRight="14.5dp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="开发时间"
                        android:textColor="#999999"
                        android:textSize="14sp" />

                    <TextView
                        android:id="@+id/tv_act_rjhj_dwck_time"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="right"
                        android:text="0000-00-00"
                        android:textColor="#333333"
                        android:textSize="14sp" />

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:layout_marginLeft="14.5dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginRight="14.5dp"
                    android:background="#999999" />
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_act_rjhj_dwck_gczt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="30dp"
            android:layout_marginLeft="14.5dp"
            android:layout_marginRight="14.5dp"
            android:gravity="center"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="公厕状态"
                android:textColor="#999999"
                android:textSize="14sp" />

            <LinearLayout
                android:id="@+id/ll_act_rjhj_dwck_gczttab"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tv_act_rjhj_dwck_gczttab"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:text="xx"
                    android:textColor="#333333"
                    android:textSize="14sp" />

                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="15dp"
                    android:layout_marginTop="6dp"
                    android:src="@drawable/inverted_triangle_style_blue" />
            </LinearLayout>


        </LinearLayout>

        <View
            android:id="@+id/view_act_rjhj_dwck_gczt"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_marginLeft="14.5dp"
            android:layout_marginRight="14.5dp"
            android:background="#999999" />

        <LinearLayout
            android:id="@+id/ll_act_rjhj_dwck_gcsx"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="30dp"
            android:layout_marginLeft="14.5dp"
            android:layout_marginRight="14.5dp"
            android:gravity="center"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="公厕属性"
                android:textColor="#999999"
                android:textSize="14sp" />

            <LinearLayout
                android:id="@+id/ll_act_rjhj_dwck_gcsxtab"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tv_act_rjhj_dwck_gcsxtab"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:text="xx"
                    android:textColor="#333333"
                    android:textSize="14sp" />

                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="15dp"
                    android:layout_marginTop="6dp"
                    android:src="@drawable/inverted_triangle_style_blue" />
            </LinearLayout>


        </LinearLayout>

        <View
            android:id="@+id/view_act_rjhj_dwck_gcsx"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_marginLeft="14.5dp"
            android:layout_marginRight="14.5dp"
            android:background="#999999" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:minHeight="30dp"
            android:layout_marginLeft="14.5dp"
            android:layout_marginRight="14.5dp"
            android:gravity="center"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="备注"
                android:textColor="#999999"
                android:textSize="14sp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tv_act_rjhj_dwck_remark"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:text="xx"
                    android:textColor="#333333"
                    android:textSize="14sp" />
            </LinearLayout>


        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_marginLeft="14.5dp"
            android:layout_marginRight="14.5dp"
            android:background="#999999" />

        <View
            android:layout_width="match_parent"
            android:layout_height="10dp"
            android:background="#f6f7fb"
            android:layout_marginTop="15dp"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="图片详情"
            android:layout_marginTop="11.5dp"
            android:layout_marginLeft="14.5dp"
            android:textColor="#333333"
            android:textSize="14sp"
            />

        <RelativeLayout
            android:descendantFocusability="blocksDescendants"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="14dp"
            android:layout_marginLeft="14.5dp"
            android:layout_marginRight="11.5dp">
            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/rlv_act_rjhj_dwck_upload_image"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
        </RelativeLayout>

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="#999999"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="64dp"
        >

        <Button
            android:id="@+id/bt_act_rjhj_dwck_confirm"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="39dp"
            android:background="@drawable/bt_actiive_409eff"
            android:text="确定"
            android:textSize="16sp"
            android:textColor="@color/white"
            android:layout_marginLeft="14.5dp"
            android:layout_marginRight="15dp"
            android:layout_gravity="center"
            />

        <Button
            android:id="@+id/bt_act_rjhj_dwck_delete"
            android:layout_weight="2"
            android:layout_width="match_parent"
            android:layout_height="39dp"
            android:background="@drawable/bt_actiive_tou_kuangff660f"
            android:text="删除"
            android:textSize="16sp"
            android:textColor="#FF660F"
            android:layout_marginRight="16dp"
            android:layout_gravity="center" />

    </LinearLayout>

    

</LinearLayout>