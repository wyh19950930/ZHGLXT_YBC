<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:gravity="center"
    android:layout_margin="10dp"
    android:background="@color/white">

    <RelativeLayout
        android:layout_gravity="center_horizontal"
        android:layout_width="300dp"
        android:layout_height="40dp">
        <TextView
            android:layout_centerInParent="true"
            android:textSize="16sp"
            android:textColor="#000000"
            android:text="添加项目"
            android:gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="#cccccc"/>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="项目名称"
                android:textColor="#333333"
                android:gravity="center"/>

            <EditText
                android:id="@+id/pop_add_project_et_xmnc"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="right"
                android:singleLine="true"
                />

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="项目类别"
                android:textColor="#333333"
                android:gravity="center"/>

            <EditText
                android:id="@+id/pop_add_project_et_xmlb"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="right"
                android:singleLine="true"
                />

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="用地面积"
                android:textColor="#333333"
                android:gravity="center"/>

            <EditText
                android:id="@+id/pop_add_project_et_ydmj"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="right"
                android:inputType="number|numberDecimal"
                android:maxLength="11"
                android:singleLine="true"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="万㎡  "
                android:textColor="#333333"
                android:gravity="center"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="征地面积"
                android:textColor="#333333"
                android:gravity="center"/>

            <EditText
                android:id="@+id/pop_add_project_et_zdmj"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="right"
                android:inputType="number|numberDecimal"
                android:maxLength="11"
                android:singleLine="true"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="万㎡"
                android:textColor="#333333"
                android:gravity="center"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="地上物"
                android:textColor="#333333"
                android:gravity="center"/>

            <EditText
                android:id="@+id/pop_add_project_et_dsw"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="right"
                android:inputType="number|numberDecimal"
                android:maxLength="11"
                android:singleLine="true"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="万㎡  "
                android:textColor="#333333"
                android:gravity="center"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="待转人口"
                android:textColor="#333333"
                android:gravity="center"/>

            <EditText
                android:id="@+id/pop_add_project_et_dzrk"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="right"
                android:inputType="number|numberDecimal"
                android:maxLength="11"
                android:singleLine="true"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="人"
                android:textColor="#333333"
                android:gravity="center"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="投资主体"
                android:textColor="#333333"
                android:gravity="center"/>

            <EditText
                android:id="@+id/pop_add_project_et_tzzt"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="right"
                android:singleLine="true"
                />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="投资方式"
                android:textColor="#333333"
                android:gravity="center"/>

            <Spinner
                android:id="@+id/pop_add_project_spr_tzfs"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            </Spinner>

        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:background="#cccccc"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="资金需求"
                android:textColor="#333333"
                android:gravity="center"/>

            <EditText
                android:id="@+id/pop_add_project_et_zjxq"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="right"
                android:inputType="number|numberDecimal"
                android:singleLine="true"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="万㎡"
                android:textColor="#333333"
                android:gravity="center"/>
        </LinearLayout>

    </LinearLayout>


    <View
        android:background="#cccccc"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginTop="30dp"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/pop_add_project_confirm"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:text="确定"
            android:textColor="#12C483"
            android:textSize="16sp"
            android:gravity="center"/>
        <View
            android:layout_width="1dp"
            android:layout_height="18dp"
            android:layout_gravity="center"
            android:background="#E0E0E0"/>
        <TextView
            android:id="@+id/pop_add_project_close"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:text="取消"
            android:textColor="#999999"
            android:textSize="16sp"
            android:gravity="center"/>
    </LinearLayout>

</LinearLayout>