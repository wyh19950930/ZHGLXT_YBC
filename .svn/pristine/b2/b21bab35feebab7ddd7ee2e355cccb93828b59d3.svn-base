package com.jymj.zhglxt.rjhj.bean.zhenfirst;

import java.math.BigDecimal;

public class WtlxfbZztBean {

    private String name;//问题名
    private int wts;//问题数
    private int zgs;//整改数
    private BigDecimal zgl;//整改率
    private String zyc;//最优村
    private BigDecimal zyczgl;//最优村整改率
    private String zcc;//最差村
    private BigDecimal zcczgl;//最差村整改率

    public WtlxfbZztBean(String name, int wts, int zgs, BigDecimal zgl, String zyc, BigDecimal zyczgl, String zcc, BigDecimal zcczgl) {
        this.name = name;
        this.wts = wts;
        this.zgs = zgs;
        this.zgl = zgl;
        this.zyc = zyc;
        this.zyczgl = zyczgl;
        this.zcc = zcc;
        this.zcczgl = zcczgl;
    }

    public String getName() {
        return name==null?"":name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public int getWts() {
        return wts;
    }

    public void setWts(int wts) {
        this.wts = wts;
    }

    public int getZgs() {
        return zgs;
    }

    public void setZgs(int zgs) {
        this.zgs = zgs;
    }

    public BigDecimal getZgl() {
        return zgl==null? BigDecimal.ZERO:zgl;
    }

    public void setZgl(BigDecimal zgl) {
        this.zgl = zgl;
    }

    public String getZyc() {
        return zyc==null?"":zyc;
    }

    public void setZyc(String zyc) {
        this.zyc = zyc;
    }

    public BigDecimal getZyczgl() {
        return zyczgl==null? BigDecimal.ZERO:zyczgl;
    }

    public void setZyczgl(BigDecimal zyczgl) {
        this.zyczgl = zyczgl;
    }

    public String getZcc() {
        return zcc==null?"":zcc;
    }

    public void setZcc(String zcc) {
        this.zcc = zcc;
    }

    public BigDecimal getZcczgl() {
        return zcczgl==null? BigDecimal.ZERO:zcczgl;
    }

    public void setZcczgl(BigDecimal zcczgl) {
        this.zcczgl = zcczgl;
    }
}
