                    android:textSize="16sp" />

                <View
                    android:id="@+id/v2_frag_zjd_tdly"
                    android:layout_width="50dp"
                    android:layout_height="2.5dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="2dp"
                    android:background="@color/white"
                    android:visibility="gone" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_frag_zjd_tdly_pie"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <com.jymj.zhglxt.widget.zdybj.MeTitleLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:attr_color="#4CA2FE"
                app:attr_isshow_right="false"
                app:attr_name="统计" />

            <com.github.mikephil.charting.charts.PieChart
                android:id="@+id/pie_frag_zjd_tdly"
                android:layout_width="match_parent"
                android:layout_height="250dp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_frag_zjd_tdly_list"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:visibility="gone">

            <com.jymj.zhglxt.widget.zdybj.MeTitleLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:attr_color="#4CA2FE"
                app:attr_isshow_right="false"
                app:attr_name="列表" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:descendantFocusability="blocksDescendants">

                <com.jymj.zhglxt.widget.OptimizedRecyclerView
                    android:id="@+id/rlv_frag_zjd_tdly"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp" />
            </RelativeLayout>
        </LinearLayout>

    </LinearLayout>
</LinearLayout>
