<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:focusable="true"
    android:focusableInTouchMode="true"
    android:orientation="vertical">
    <LinearLayout
        android:background="@drawable/mapback_ffffff"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_marginTop="5dp"
            android:layout_marginRight="13dp"
            android:layout_marginLeft="14.5dp"
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:gravity="center">
            <TextView
                android:id="@+id/item_xxcj_jcqt_name"
                android:textSize="16sp"
                android:textColor="#333333"
                android:text="王娟"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <ImageView
                android:id="@+id/item_xxcj_jcqt_sex"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/sex_woman_icon"
                android:layout_marginLeft="9.5dp"/>

            <TextView
                android:id="@+id/item_xxcj_jcqt_age"
                android:textSize="12sp"
                android:textColor="#666666"
                android:text="24岁"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="13.5dp"/>
            <TextView
                android:id="@+id/item_xxcj_jcqt_education"
                android:textSize="12sp"
                android:textColor="#666666"
                android:text="本科学历"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="right"/>
        </LinearLayout>
        <LinearLayout
            android:layout_marginRight="13dp"
            android:layout_marginLeft="14.5dp"
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:gravity="center">
            <TextView
                android:id="@+id/item_xxcj_jcqt_idcard"
                android:text="110110110111011101"
                android:textColor="#666666"
                android:textSize="12sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

            <TextView
                android:id="@+id/item_xxcj_jcqt_industry"
                android:textSize="12sp"
                android:textColor="#666666"
                android:text="快递行业"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="right"/>
        </LinearLayout>
        <LinearLayout
            android:layout_marginRight="13dp"
            android:layout_marginLeft="14.5dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:gravity="center">
            <TextView
                android:id="@+id/item_xxcj_jcqt_address"
                android:text="河北省涿州市某镇某小区"
                android:textColor="#333333"
                android:textSize="12sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

            <TextView
                android:id="@+id/item_xxcj_jcqt_birthday"
                android:textSize="12sp"
                android:textColor="#666666"
                android:text="2022-01-02"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="right"/>
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="#e0e0e0"
            android:layout_marginTop="5dp"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="34.5dp"
            android:orientation="horizontal"
            android:gravity="center">

            <TextView
                android:id="@+id/item_xxcj_jcqt_update"
                android:gravity="center"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="修改"
                android:textSize="16sp"
                android:textColor="#FF660F"
                />
            <View
                android:layout_width="0.5dp"
                android:layout_height="18dp"
                android:background="#e0e0e0"/>
            <TextView
                android:visibility="gone"
                android:id="@+id/item_xxcj_jcqt_delete"
                android:gravity="center"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="删除"
                android:textSize="16sp"
                android:textColor="#FF5458"
                />
            <TextView
                android:id="@+id/item_xxcj_jcqt_lc"
                android:gravity="center"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="流出"
                android:textSize="16sp"
                android:textColor="#9CCC3D"
                />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="5dp"
            android:background="#F6F7FB"/>
    </LinearLayout>

</LinearLayout>