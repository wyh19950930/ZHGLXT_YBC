<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".zjd.activity.AddUserActivity">

    <RelativeLayout
        android:background="@color/blue"
        android:layout_width="match_parent"
        android:layout_height="50dp">
        <ImageView
            android:id="@+id/iv_add_user_back"
            android:layout_marginLeft="10dp"
            android:layout_centerVertical="true"
            android:src="@drawable/left_arrow"
            android:layout_width="30dp"
            android:layout_height="30dp"/>
        <TextView
            android:textStyle="bold"
            android:textSize="18sp"
            android:textColor="@color/white"
            android:layout_centerInParent="true"
            android:text="用户"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

        <ImageView
            android:id="@+id/iv_delete_user"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:src="@drawable/delete" />
    </RelativeLayout>
    <LinearLayout
        android:background="@color/white"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:visibility="gone"
            android:gravity="center_vertical"
            android:layout_marginLeft="10dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="40dp">
            <TextView
                android:gravity="right"
                android:textSize="15sp"
                android:textColor="@color/black"
                android:minWidth="80dp"
                android:text="真实姓名*："
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <EditText
                android:textSize="14sp"
                android:id="@+id/et_add_user_name"
                android:background="@null"
                android:minWidth="200dp"
                android:hint="请输入真实姓名"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </LinearLayout>
        <View
            android:background="@color/gray"
            android:layout_width="match_parent"
            android:layout_height="0.1dp"/>
        <LinearLayout
            android:gravity="center_vertical"
            android:layout_marginLeft="10dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="40dp">
            <TextView
                android:gravity="right"
                android:textSize="15sp"
                android:textColor="@color/black"
                android:minWidth="80dp"
                android:text="用户名："
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
<!--            (默认手机号)-->
            <EditText
                android:textSize="14sp"
                android:id="@+id/et_add_user_username"
                android:background="@null"
                android:minWidth="200dp"
                android:hint="请输入用户名"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </LinearLayout>

        <View
            android:background="@color/gray"
            android:layout_width="match_parent"
            android:layout_height="0.1dp"/>
        <LinearLayout
            android:gravity="center_vertical"
            android:layout_marginLeft="10dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="40dp">
            <TextView
                android:gravity="right"
                android:textSize="15sp"
                android:textColor="@color/black"
                android:minWidth="80dp"
                android:text="手机号："
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <EditText
                android:textSize="14sp"
                android:inputType="phone|number"
                android:digits="1234567890"
                android:maxLength="11"
                android:singleLine="true"
                android:id="@+id/et_add_user_phone"
                android:background="@null"
                android:minWidth="200dp"
                android:hint="请输入手机号"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </LinearLayout>
        <View
            android:background="@color/gray"
            android:layout_width="match_parent"
            android:layout_height="0.1dp"/>
        <LinearLayout
            android:gravity="center_vertical"
            android:layout_marginLeft="10dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="40dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:gravity="right"
                android:minWidth="80dp"
                android:text="密码："
                android:textColor="@color/black"
                android:textSize="15sp" />
            <EditText
                android:textSize="14sp"
                android:text="cs123"
                android:id="@+id/et_add_user_password"
                android:background="@null"
                android:minWidth="200dp"
                android:hint="请输入密码"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </LinearLayout>
        <View
            android:background="@color/gray"
            android:layout_width="match_parent"
            android:layout_height="0.1dp"/>
        <LinearLayout
            android:id="@+id/ll_add_user_lx"
            android:gravity="center_vertical"
            android:layout_marginLeft="10dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="40dp">
            <TextView
                android:gravity="right"
                android:textSize="15sp"
                android:textColor="@color/black"
                android:minWidth="80dp"
                android:text="类型："
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <CheckBox
                android:id="@+id/box_add_user_bc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="百村"
                android:textSize="14sp" />
            <CheckBox
                android:id="@+id/box_add_user_qu"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="区"
                android:textSize="14sp" />
            <CheckBox
                android:id="@+id/box_add_user_zhen"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="镇"
                android:textSize="14sp" />
            <CheckBox
                android:checked="true"
                android:id="@+id/box_add_user_cun"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="村"
                android:textSize="14sp" />
            <!--<TextView
                android:textSize="14sp"
                android:visibility="gone"
                android:layout_marginLeft="10dp"
                android:id="@+id/box_add_user_lx"
                android:text="内业"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

            <CheckBox
                android:id="@+id/box_add_user_ny"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="内业"
                android:textSize="14sp" />

            <CheckBox
                android:textSize="14sp"
                android:checked="true"
                android:id="@+id/box_add_user_cun"
                android:text="村"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <CheckBox
                android:textSize="14sp"
                android:id="@+id/box_add_user_zhen"
                android:text="镇"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <CheckBox
                android:textSize="14sp"
                android:id="@+id/box_add_user_wy"
                android:text="外业"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>-->

        </LinearLayout>
        <View
            android:background="@color/gray"
            android:layout_width="match_parent"
            android:layout_height="0.1dp"/>
        <LinearLayout
            android:id="@+id/ll_add_user_sfld"
            android:gravity="center_vertical"
            android:layout_marginLeft="10dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="40dp">
            <TextView
                android:gravity="right"
                android:textSize="15sp"
                android:textColor="@color/black"
                android:minWidth="80dp"
                android:text="权限："
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <com.jymj.zhglxt.util.RadioGroupEx
                android:id="@+id/rge_add_user_sfld"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <RadioButton
                    android:id="@+id/rb_add_user_sfld_shi"
                    android:text="领导"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
                <RadioButton
                    android:id="@+id/rb_add_user_sfld_fou"
                    android:checked="true"
                    android:text="操作员"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
                <RadioButton
                    android:id="@+id/rb_add_user_dcy"
                    android:text="调查员"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </com.jymj.zhglxt.util.RadioGroupEx>
        </LinearLayout>
        <View
            android:background="@color/gray"
            android:layout_width="match_parent"
            android:layout_height="0.1dp"/>
        <LinearLayout
            android:id="@+id/ll_add_user_sfzc"
            android:gravity="center_vertical"
            android:layout_marginLeft="10dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="40dp">
            <TextView
                android:gravity="right"
                android:textSize="15sp"
                android:textColor="@color/black"
                android:minWidth="80dp"
                android:text="是否正常："
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <com.jymj.zhglxt.util.RadioGroupEx
                android:id="@+id/rge_add_user_sfzc"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <RadioButton
                    android:id="@+id/rb_add_user_sfzc_shi"
                    android:checked="true"
                    android:text="是"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
                <RadioButton
                    android:id="@+id/rb_add_user_sfzc_fou"
                    android:text="否"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </com.jymj.zhglxt.util.RadioGroupEx>
        </LinearLayout>
        <View
            android:background="@color/gray"
            android:layout_width="match_parent"
            android:layout_height="0.1dp"/>
        <LinearLayout
            android:visibility="gone"
            android:gravity="center_vertical"
            android:layout_marginLeft="10dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="40dp">
            <TextView
                android:gravity="right"
                android:textSize="15sp"
                android:textColor="@color/black"
                android:minWidth="80dp"
                android:text="身份证号："
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <EditText
                android:textSize="14sp"
                android:id="@+id/et_add_user_id_card"
                android:background="@null"
                android:minWidth="200dp"
                android:hint="请输入身份证号"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </LinearLayout>
        <View
            android:background="@color/gray"
            android:layout_width="match_parent"
            android:layout_height="0.1dp"/>
        <LinearLayout
            android:visibility="gone"
            android:gravity="center_vertical"
            android:layout_marginLeft="10dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="40dp">
            <TextView
                android:gravity="right"
                android:textSize="15sp"
                android:textColor="@color/black"
                android:minWidth="80dp"
                android:text="职务："
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <EditText
                android:textSize="14sp"
                android:id="@+id/et_add_user_zw"
                android:background="@null"
                android:minWidth="200dp"
                android:hint="请输入职务"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </LinearLayout>
        <View
            android:background="@color/gray"
            android:layout_width="match_parent"
            android:layout_height="0.1dp"/>
        <LinearLayout
            android:id="@+id/ll_add_user_qu"
            android:gravity="center_vertical"
            android:layout_marginLeft="10dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="40dp">
            <TextView
                android:gravity="right"
                android:textSize="15sp"
                android:textColor="@color/black"
                android:minWidth="80dp"
                android:text="区："
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <TextView
                android:textSize="14sp"
                android:id="@+id/et_add_user_qu"
                android:gravity="center_vertical"
                android:text="请选择区"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent"/>
            <ImageView
                android:layout_marginRight="10dp"
                android:src="@drawable/back_right_icon"
                android:layout_width="20dp"
                android:layout_height="20dp"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/ll_add_user_zhen"
            android:gravity="center_vertical"
            android:layout_marginLeft="10dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="40dp">
            <TextView
                android:gravity="right"
                android:textSize="15sp"
                android:textColor="@color/black"
                android:minWidth="80dp"
                android:text="镇名："
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <TextView
                android:textSize="14sp"
                android:id="@+id/et_add_user_zhen"
                android:gravity="center_vertical"
                android:text="请选择镇"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent"/>
            <ImageView
                android:layout_marginRight="10dp"
                android:src="@drawable/back_right_icon"
                android:layout_width="20dp"
                android:layout_height="20dp"/>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/ll_add_user_cun"
            android:gravity="center_vertical"
            android:layout_marginLeft="10dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="40dp">
            <TextView
                android:gravity="right"
                android:textSize="15sp"
                android:textColor="@color/black"
                android:minWidth="80dp"
                android:text="村名："
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <TextView
                android:textSize="14sp"
                android:id="@+id/et_add_user_cun"
                android:gravity="center_vertical"
                android:text="请选择村"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent"/>
            <ImageView
                android:layout_marginRight="10dp"
                android:src="@drawable/back_right_icon"
                android:layout_width="20dp"
                android:layout_height="20dp"/>
        </LinearLayout>
<!--        blue_button_background-->
        <Button
            android:id="@+id/et_add_user_save"
            android:textSize="15sp"
            android:textColor="@color/white"
            android:background="@drawable/mapback_449eef"
            android:layout_marginTop="60dp"
            android:layout_gravity="center_horizontal"
            android:text="保存"
            android:layout_width="300dp"
            android:layout_height="50dp"/>


    </LinearLayout>


</LinearLayout>
