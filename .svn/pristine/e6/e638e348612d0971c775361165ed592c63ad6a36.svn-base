<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/ll_table_zjdfj"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:gravity="center"
        android:orientation="horizontal">
<!--        style="@style/table_title_textview"-->
        <TextView
            android:id="@+id/tv_item_zjdfj_bh"

            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:textColor="#333333"
            android:layout_marginLeft="14dp"
            android:text="编号" />

        <TextView
            android:id="@+id/tv_item_zjdfj_cm"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:textColor="#333333"
            android:text="村名" />

        <TextView
            android:id="@+id/tv_item_zjdfj_hzmc"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:textColor="#666666"
            android:text="户主名称" />
        <TextView
            android:visibility="gone"
            android:id="@+id/tv_item_zjdfj_mph"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:textColor="#666666"
            android:text="地址" />
        <TextView
            android:visibility="gone"
            android:id="@+id/tv_zjdfjgl_hb"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:textColor="#666666"
            android:text="户别" />
        <TextView
            android:id="@+id/tv_item_zjdfj_jlsj"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:textColor="#666666"
            android:text="申请时间" />
        <TextView
            android:id="@+id/tv_item_zjdfj_lxfs"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:textColor="#666666"
            android:text="联系方式" />

        <LinearLayout
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:orientation="horizontal"
            android:background="#F6F7FB"
            android:gravity="center">
            <ImageView
                android:src="@drawable/back_right_icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"/>
        </LinearLayout>
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="#333333"
        android:layout_marginLeft="14dp"/>


</LinearLayout>