<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="10dp"
    android:background="@drawable/bt_actiive_xaiyoubai_white"
    android:gravity="center"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="@dimen/margin_zdkd"
        android:layout_height="40dp"
        android:layout_gravity="center_horizontal"
        android:background="@drawable/bt_actiive_shangbai_blue2">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:gravity="center"
            android:text="家庭社会保障与财政需求情况"
            android:textColor="@color/white"
            android:textSize="16sp" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="#cccccc" />


    <com.jymj.zhglxt.widget.SizeLimitView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:maxHeight="@dimen/margin_zdgd">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/margin_bggd"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="@dimen/margin_sxjg"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="@dimen/margin_sxjg"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="3"
                        android:gravity="center_vertical"
                        android:text="参加公益岗位人数"
                        android:textColor="#333333" />

                    <EditText
                        android:id="@+id/pop_add_nhjtjc_jtshbzyczxqqk_et_cjgygwrs"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="2"
                        android:background="@drawable/shape_list_table_title2"
                        android:gravity="center_vertical"
                        android:hint="请输入参加公益岗位人数"
                        android:inputType="number"
                        android:maxLength="11"
                        android:singleLine="true"
                        android:textColorHint="#00000000" />

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/margin_bggd"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="@dimen/margin_sxjg"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="@dimen/margin_sxjg"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="3"
                        android:gravity="center_vertical"
                        android:text="获得农业政策性补贴（元）"
                        android:textColor="#333333" />

                    <EditText
                        android:id="@+id/pop_add_nhjtjc_jtshbzyczxqqk_et_hdnyzcxbt"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="2"
                        android:background="@drawable/shape_list_table_title2"
                        android:gravity="center_vertical"
                        android:hint="请输入获得农业政策性补贴"
                        android:inputType="number"
                        android:maxLength="11"
                        android:singleLine="true"
                        android:textColorHint="#00000000" />

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/margin_bggd"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="@dimen/margin_sxjg"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="@dimen/margin_sxjg"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="3"
                        android:gravity="center_vertical"
                        android:text="合作社盈余返还（元）"
                        android:textColor="#333333" />

                    <EditText
                        android:id="@+id/pop_add_nhjtjc_jtshbzyczxqqk_et_hzsyyfh"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="2"
                        android:background="@drawable/shape_list_table_title2"
                        android:gravity="center_vertical"
                        android:hint="请输入合作社盈余返还"
                        android:inputType="number"
                        android:maxLength="11"
                        android:singleLine="true"
                        android:textColorHint="#00000000" />

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/margin_bggd"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="@dimen/margin_sxjg"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="@dimen/margin_sxjg"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="3"
                        android:gravity="center_vertical"
                        android:text="参加城镇职工基本医疗保险人数"
                        android:textColor="#333333" />

                    <EditText
                        android:id="@+id/pop_add_nhjtjc_jtshbzyczxqqk_et_czzgjbylbxrs"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="2"
                        android:background="@drawable/shape_list_table_title2"
                        android:gravity="center_vertical"
                        android:hint="请输入合作社盈余返还"
                        android:inputType="number"
                        android:maxLength="11"
                        android:singleLine="true"
                        android:textColorHint="#00000000" />

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/margin_bggd"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="@dimen/margin_sxjg"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="@dimen/margin_sxjg"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="3"
                        android:gravity="center_vertical"
                        android:text="参加城乡基本医疗保险人数"
                        android:textColor="#333333" />

                    <EditText
                        android:id="@+id/pop_add_nhjtjc_jtshbzyczxqqk_et_gxjbylbxrs"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="2"
                        android:background="@drawable/shape_list_table_title2"
                        android:gravity="center_vertical"
                        android:hint="请输入参加城乡基本医疗保险人数"
                        android:inputType="number"
                        android:maxLength="11"
                        android:singleLine="true"
                        android:textColorHint="#00000000" />

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/margin_bggd"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="@dimen/margin_sxjg"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="@dimen/margin_sxjg"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="3"
                        android:gravity="center_vertical"
                        android:text="参加城镇职工基本养老保险人数"
                        android:textColor="#333333" />

                    <EditText
                        android:id="@+id/pop_add_nhjtjc_jtshbzyczxqqk_et_czzgjbyanglbxrs"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="2"
                        android:background="@drawable/shape_list_table_title2"
                        android:gravity="center_vertical"
                        android:hint="请输入参加城镇职工基本养老保险人数"
                        android:inputType="number"
                        android:maxLength="11"
                        android:singleLine="true"
                        android:textColorHint="#00000000" />

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/margin_bggd"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="@dimen/margin_sxjg"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="@dimen/margin_sxjg"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="3"
                        android:gravity="center_vertical"
                        android:text="参加城乡基本养老保险人数"
                        android:textColor="#333333" />

                    <EditText
                        android:id="@+id/pop_add_nhjtjc_jtshbzyczxqqk_et_cxjbylbxrs"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="2"
                        android:background="@drawable/shape_list_table_title2"
                        android:gravity="center_vertical"
                        android:hint="请输入参加城乡基本养老保险人数"
                        android:inputType="number"
                        android:maxLength="11"
                        android:singleLine="true"
                        android:textColorHint="#00000000" />

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="@dimen/margin_sxjg"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="@dimen/margin_sxjg"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="3"
                        android:gravity="center_vertical"
                        android:text="最需要的财政支持项目
（选项见备注）"
                        android:textColor="#333333" />

                    <EditText
                        android:maxLength="1"
                        android:id="@+id/pop_add_nhjtjc_jtshbzyczxqqk_et_zxyczzcxm"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="2"
                        android:background="@drawable/shape_list_table_title2"
                        android:gravity="center_vertical"
                        android:hint="请输入最需要的财政支持项目"
                        android:digits="abcdefghiABCDEFGHI"
                        android:textColorHint="#00000000" />
                    <LinearLayout
                        android:visibility="gone"
                        android:id="@+id/pop_add_nhjtjc_jtshbzyczxqqk_ll_czzcxm"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="2"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:gravity="center_vertical"
                            android:text="I:"/>
                        <EditText
                            android:id="@+id/pop_add_nhjtjc_jtshbzyczxqqk_et_czzcxm1"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/shape_list_table_title2"
                            android:gravity="center_vertical"
                            android:textSize="10sp"
                            android:singleLine="true"
                            android:hint="输入其他参政支持项目"/>
                    </LinearLayout>

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="@dimen/margin_sxjg"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="@dimen/margin_sxjg"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="3"
                        android:gravity="center_vertical"
                        android:text="最需要的培训项目
（选项见备注）"
                        android:textColor="#333333" />

                    <EditText
                        android:maxLength="1"
                        android:id="@+id/pop_add_nhjtjc_jtshbzyczxqqk_et_zxypxxm"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="2"
                        android:background="@drawable/shape_list_table_title2"
                        android:gravity="center_vertical"
                        android:digits="abcdefABCDEF"
                        android:hint="请输入最需要的培训项目"
                        android:textColorHint="#00000000" />
                    <LinearLayout
                        android:visibility="gone"
                        android:id="@+id/pop_add_nhjtjc_jtshbzyczxqqk_ll_zxypxxm"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="2"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:gravity="center_vertical"
                            android:text="F:"/>
                        <EditText
                            android:id="@+id/pop_add_nhjtjc_jtshbzyczxqqk_et_zxypxxm1"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/shape_list_table_title2"
                            android:gravity="center_vertical"
                            android:textSize="10sp"
                            android:singleLine="true"
                            android:hint="输入其他培训项目"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="@dimen/margin_sxjg"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="@dimen/margin_sxjg"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="3"
                        android:gravity="center_vertical"
                        android:text="获取政策信息的主要渠道
（选项见备注）"
                        android:textColor="#333333" />

                    <EditText
                        android:maxLength="1"
                        android:id="@+id/pop_add_nhjtjc_jtshbzyczxqqk_et_hqzcxxzyqd"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="2"
                        android:background="@drawable/shape_list_table_title2"
                        android:gravity="center_vertical"
                        android:digits="abcdefghABCDEFGH"
                        android:hint="请输入获取政策信息的主要渠道"
                        android:textColorHint="#00000000" />

                    <LinearLayout
                        android:visibility="gone"
                        android:id="@+id/pop_add_nhjtjc_jtshbzyczxqqk_ll_hqzcxxzyqd"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="2"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:gravity="center_vertical"
                            android:text="H:"/>
                        <EditText
                            android:id="@+id/pop_add_nhjtjc_jtshbzyczxqqk_et_hqzcxxzyqd1"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="2"
                            android:background="@drawable/shape_list_table_title2"
                            android:gravity="center_vertical"
                            android:textSize="10sp"
                            android:singleLine="true"
                            android:hint="输入其他渠道"
                            android:textColorHint="#00000000" />
                    </LinearLayout>
                </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:text="注：" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:text="[1]最需要的财政支持项目： A 改水改厕、 B 农村能源节能改造、 C 乡村道路建设、 D 医疗保障、 E 养老保障、 F 就业培训、 G 支持农民合作社发展、 H 扶持壮大集体经济、 I 其他" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:text="[2]最需要的培训项目：A 农业技能、 B 企业上岗培训、 C 农产品销售能力、 D 乡村旅游等服务业培训、 E 基础技能培训、（计算机、外语等）、 F 其他" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:text="[3]获取政策信息的主要渠道：A 电视、广播、 B 抖音、公众号等新媒体、 C 电话手机、 D 报纸杂志、 E 社区展板、 F 村干部传达、 G 亲戚朋友、 H 其他" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:text="[4]最后三列问题为多选，请选三项，并按重要程度排序。" />

            </LinearLayout>


        </ScrollView>
    </com.jymj.zhglxt.widget.SizeLimitView>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginTop="5dp"
        android:background="#cccccc" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/pop_add_nhjtjc_jtshbzyczxqqk_confirm"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@drawable/bt_actiive_xaizuobai_blue"
            android:gravity="center"
            android:text="确定"
            android:textColor="#ffffff"
            android:textSize="16sp" />

        <View
            android:layout_width="1dp"
            android:layout_height="18dp"
            android:layout_gravity="center"
            android:background="#E0E0E0"
            android:visibility="gone" />

        <TextView
            android:id="@+id/pop_add_nhjtjc_jtshbzyczxqqk_close"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:text="取消"
            android:textColor="#4CA2FE"
            android:textSize="16sp" />
    </LinearLayout>

</LinearLayout>