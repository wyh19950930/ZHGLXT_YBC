<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="attrs_info">
        <attr name="attr_key" format="string"/>
        <attr name="attr_hint" format="string"/>
        <attr name="attr_value" format="string"/>
<!--        控制展示收起-->
        <attr name="attr_src" format="reference"/>
        <attr name="attr_name" format="string"/>
        <attr name="attr_color" format="color"/>
        <attr name="attr_leftname" format="string"/>
        <attr name="attr_leftcolor" format="color"/>
        <attr name="attr_hidename" format="string"/>
        <attr name="attr_leftsrc" format="reference"/>
        <attr name="attr_isshow_right" format="boolean"/>

        <attr name="attr_time" format="reference"/>
        <attr name="attr_arrows" format="reference"/>
        <attr name="attr_isint" format="boolean"/>
        <attr name="attr_isfloat" format="boolean"/>
        <attr name="attr_ischeck" format="boolean"/>
        <attr name="attr_isleftcheck" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="SizeLimitView">
        <!--最大高度-->
        <attr name="maxHeight"  format = "dimension" />
        <!--宽高比-->
        <attr name="widthHeightRatio"  format = "float" />
        <!--是否通过宽度限制高度比率-->
        <attr name="limitHeightByWidth"  format = "boolean" />
        <!--是否通过高度限制宽度比率-->
        <attr name="limitWidthByHeight"  format = "boolean" />
    </declare-styleable>
  <!--  <declare-styleable name="attrs_title">
        <attr name="attr_src" format="reference"/>
        <attr name="attr_name" format="string"/>
    </declare-styleable>-->
</resources>
