<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/rl_pop_modify"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:background="@drawable/mapback_ffffff"
        android:orientation="vertical">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:gravity="center"
            android:text="新增成员"
            android:textColor="#333333"
            android:textSize="16sp"
            android:textStyle="bold" />

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_marginTop="10dp"
            android:background="#e0e0e0" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="14.5dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="12.5dp"
            android:gravity="center"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="姓名"
                android:textColor="#999999"
                android:textSize="14sp" />

            <EditText
                android:id="@+id/edt_jtxxqt_name"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@null"
                android:gravity="right|center"
                android:hint="请输入姓名"
                android:inputType="text"
                android:textColor="#333333"
                android:textSize="14sp"
                android:singleLine="true"/>

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_marginLeft="14.5dp"
            android:layout_marginRight="12.5dp"
            android:background="#eeeeee" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="14.5dp"
            android:layout_marginRight="12.5dp"
            android:gravity="center"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="性别"
                android:textColor="#999999"
                android:textSize="14sp" />

            <RadioGroup
                android:id="@+id/rgp_jtxxqt_sex"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="right|center"
                android:orientation="horizontal">

                <RadioButton
                    android:id="@+id/rbt_jtxxqt_sex_man"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:background="@drawable/radiobutton_background"
                    android:button="@null"
                    android:checked="true"
                    android:gravity="center"
                    android:text="男"
                    android:textColor="@drawable/radiobutton_textcolor"
                    android:textSize="14sp" />

                <RadioButton
                    android:id="@+id/rbt_jtxxqt_sex_woman"
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_marginLeft="20dp"
                    android:background="@drawable/radiobutton_background"
                    android:button="@null"
                    android:gravity="center"
                    android:text="女"
                    android:textColor="@drawable/radiobutton_textcolor"
                    android:textSize="14sp" />
            </RadioGroup>
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_marginLeft="14.5dp"
            android:layout_marginRight="12.5dp"
            android:background="#eeeeee" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="14.5dp"
            android:layout_marginRight="12.5dp"
            android:gravity="center"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="年龄"
                android:textColor="#999999"
                android:textSize="14sp" />

            <EditText
                android:id="@+id/edt_jtxxqt_age"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@null"
                android:gravity="right|center"
                android:hint="请输入年龄"
                android:textColor="#333333"
                android:textSize="14sp"
                android:numeric="integer"
                android:maxLength="11"
                android:singleLine="true"/>

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_marginLeft="14.5dp"
            android:layout_marginRight="12.5dp"
            android:background="#eeeeee" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="14.5dp"
            android:layout_marginRight="12.5dp"
            android:gravity="center"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="身份证号码"
                android:textColor="#999999"
                android:textSize="14sp" />

            <EditText
                android:id="@+id/edt_jtxxqt_idcard"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@null"
                android:gravity="right|center"
                android:hint="请输入身份证号码"
                android:textColor="#333333"
                android:textSize="14sp"
                android:digits="0123456789qwertyuioplkjhgfdsazxcvbnm"
                android:singleLine="true"/>

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_marginLeft="14.5dp"
            android:layout_marginRight="12.5dp"
            android:background="#eeeeee" />

        <LinearLayout
            android:id="@+id/ll_jtxxqt_hjdz"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="14.5dp"
            android:layout_marginRight="12.5dp"
            android:gravity="center"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="户籍地址"
                android:textColor="#999999"
                android:textSize="14sp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="right"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tv_jtxxqt_hjdz_province"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@null"
                    android:gravity="right"
                    android:hint="省"
                    android:textColor="#999999"
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/tv_jtxxqt_hjdz_city"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@null"
                    android:gravity="right"
                    android:text="市"
                    android:textColor="#999999"
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/tv_jtxxqt_hjdz_county"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@null"
                    android:gravity="right"
                    android:text="县"
                    android:textColor="#999999"
                    android:textSize="14sp" />

            </LinearLayout>
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_marginLeft="14.5dp"
            android:layout_marginRight="12.5dp"
            android:background="#eeeeee" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="14.5dp"
            android:layout_marginRight="12.5dp"
            android:gravity="center"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="详细地址"
                android:textColor="#999999"
                android:textSize="14sp" />

            <EditText
                android:id="@+id/edt_jtxxqt_address"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@null"
                android:gravity="right|center"
                android:hint="请输入详细地址"
                android:inputType="text"
                android:textColor="#333333"
                android:textSize="14sp"
                android:singleLine="true"/>

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_marginLeft="14.5dp"
            android:layout_marginRight="12.5dp"
            android:background="#eeeeee" />

        <LinearLayout
            android:id="@+id/ll_jtxxqt_cshy"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="14.5dp"
            android:layout_marginRight="12.5dp"
            android:gravity="center"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="从事行业"
                android:textColor="#999999"
                android:textSize="14sp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="right"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tv_jtxxqt_cshy"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="12dp"
                    android:background="@null"
                    android:text="请选择"
                    android:textColor="#999999"
                    android:textSize="14sp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/back_right_icon" />
            </LinearLayout>
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_marginLeft="14.5dp"
            android:layout_marginRight="12.5dp"
            android:background="#eeeeee" />
        <LinearLayout
            android:id="@+id/ll_jtxxqt_whcd"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="14.5dp"
            android:layout_marginRight="12.5dp"
            android:gravity="center"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="文化程度"
                android:textColor="#999999"
                android:textSize="14sp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="right"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tv_jtxxqt_whcd"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="12dp"
                    android:background="@null"
                    android:text="请选择"
                    android:textColor="#999999"
                    android:textSize="14sp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/back_right_icon" />
            </LinearLayout>
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_marginLeft="14.5dp"
            android:layout_marginRight="12.5dp"
            android:background="#eeeeee" />
        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_marginTop="20dp"
            android:background="#e0e0e0" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/bt_jtxxqt_yes"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="保存"
                android:textColor="#4CA2FE"
                android:textSize="16sp" />

            <View
                android:layout_width="0.5dp"
                android:layout_height="18dp"
                android:background="#e0e0e0"
                android:layout_gravity="center" />

            <TextView
                android:id="@+id/bt_jtxxqt_no"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="取消"
                android:textColor="#999999"
                android:textSize="16sp" />

        </LinearLayout>


