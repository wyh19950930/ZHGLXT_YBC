<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.jymj.zhglxt.zjd.activity.AjlbSearchActivity">
    <LinearLayout
        android:gravity="center_vertical"
        android:background="@color/white"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="45dp">
        <ImageView
            android:id="@+id/iv_act_ajlb_search_back"
            android:padding="10dp"
            android:src="@drawable/back_left_icon"
            android:layout_width="40dp"
            android:layout_height="match_parent"/>
        <com.jymj.zhglxt.widget.ClearEditText
            android:id="@+id/cet_act_ajlb_search_ajmc"
            android:paddingLeft="5dp"
            android:paddingTop="3.5dp"
            android:paddingBottom="3.5dp"
            android:gravity="left"
            android:background="@drawable/shape_list_table_title1"
            android:layout_weight="1"
            android:hint="案件名称/案件编号"
            android:layout_width="0dp"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/tv_act_ajlb_search"
            android:layout_margin="10dp"
            android:textColor="#333333"
            android:textSize="14sp"
            android:text="搜索"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </LinearLayout>
    <LinearLayout
        android:layout_marginLeft="15dp"
        android:gravity="center_vertical"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:textColor="#333333"
            android:gravity="center_vertical"
            android:text="属地："
            android:layout_width="wrap_content"
            android:layout_height="40dp"/>
        <com.jymj.zhglxt.widget.zdybj.TagCloudLayout
            android:id="@+id/tcl_act_ajlb_search_sd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
        <com.jymj.zhglxt.widget.zdybj.TagCloudLayout
            android:layout_marginTop="50dp"
            android:id="@+id/tcl_act_ajlb_search_ajlx"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
        <TextView
            android:textColor="#333333"
            android:gravity="center_vertical"
            android:text="状态："
            android:layout_width="wrap_content"
            android:layout_height="40dp"/>
        <com.jymj.zhglxt.widget.zdybj.TagCloudLayout
            android:id="@+id/tcl_act_ajlb_search_zt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

    </LinearLayout>


</LinearLayout>
