<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/rl_pop_point"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/popback"
    android:orientation="vertical"
    android:layout_margin="10dp">
    <RelativeLayout
        android:layout_gravity="center_horizontal"
        android:layout_width="350dp"
        android:layout_height="40dp">
        <TextView
            android:textColor="#333333"
            android:layout_centerInParent="true"
            android:textSize="16sp"
            android:text="注册情况"
            android:gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </RelativeLayout>
    <View
        android:background="@color/gray"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"/>
    <LinearLayout
        android:layout_marginRight="15.5dp"
        android:layout_marginLeft="15dp"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <com.jymj.zhglxt.widget.zdybj.ZdyEditLayout
            app:attr_key="现状企业名称"
            app:attr_hint="请输入"
            android:id="@+id/zel_pop_czdw_xzqymc"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
        <com.jymj.zhglxt.widget.zdybj.ZdyEditLayout
            app:attr_key="注册地址"
            app:attr_hint="请输入"
            android:id="@+id/zel_pop_czdw_xzcdz"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
        <com.jymj.zhglxt.widget.zdybj.ZdyEditLayout
            app:attr_key="实际经营地址"
            app:attr_hint="请输入"
            android:id="@+id/zel_pop_czdw_sjjydz"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
        <com.jymj.zhglxt.widget.zdybj.ZdyTextLayout
            app:attr_key="入住日期"
            app:attr_hint="请选择时间"
            android:id="@+id/zel_pop_czdw_rzrq"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
        <com.jymj.zhglxt.widget.zdybj.ZdyTextLayout
            app:attr_key="经营执照注册日期"
            app:attr_hint="请选择时间"
            android:id="@+id/zel_pop_czdw_jyzzzcrq"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
        <com.jymj.zhglxt.widget.zdybj.ZdyEditLayout
            app:attr_key="税务登记证注册地"
            app:attr_hint="请输入"
            android:id="@+id/zel_pop_czdw_swdjzzcd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
    </LinearLayout>
    <View
        android:background="#F6F7FB"
        android:layout_width="match_parent"
        android:layout_height="10dp"/>
    <LinearLayout
        android:layout_marginRight="15.5dp"
        android:layout_marginLeft="15dp"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_weight="1"
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="wrap_content">

            <com.jymj.zhglxt.widget.zdybj.ZdyTextLayout
                app:attr_arrows="@drawable/back_right_icon"
                app:attr_key="企业类型"
                app:attr_hint="请选择"
                android:id="@+id/zel_act_czdw_qylx"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <com.jymj.zhglxt.widget.zdybj.ZdyTextLayout
                app:attr_arrows="@drawable/back_right_icon"
                app:attr_key="行业类型"
                app:attr_hint="请选择"
                android:id="@+id/zel_act_czdw_hylx"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <com.jymj.zhglxt.widget.zdybj.ZdyEditLayout
                app:attr_key="行政联系人"
                app:attr_hint="请输入"
                android:id="@+id/zel_pop_czdw_xzlxr"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <com.jymj.zhglxt.widget.zdybj.ZdyTextLayout
                app:attr_arrows="@drawable/back_right_icon"
                app:attr_key="是否转租"
                app:attr_hint="请选择"
                android:id="@+id/zel_act_czdw_sfzz"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </LinearLayout>
        <View
            android:layout_marginRight="5dp"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="10dp"
            android:background="#333333"
            android:layout_width="1dp"
            android:layout_height="match_parent"/>
        <LinearLayout
            android:layout_weight="1"
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="wrap_content">
            <com.jymj.zhglxt.widget.zdybj.ZdyTextLayout
                app:attr_arrows="@drawable/back_right_icon"
                app:attr_key="注册类型"
                app:attr_hint="请选择"
                android:id="@+id/zel_act_czdw_zclx"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <!--                app:attr_isfloat="true"-->
            <com.jymj.zhglxt.widget.zdybj.ZdyEditLayout
                app:attr_key="注册资金"
                app:attr_hint="请输入"
                android:id="@+id/zel_pop_czdw_zczj"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <com.jymj.zhglxt.widget.zdybj.ZdyEditLayout
                app:attr_isint="true"
                app:attr_key="联系电话"
                app:attr_hint="请输入"
                android:id="@+id/zel_pop_zcdw_lxdw"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <com.jymj.zhglxt.widget.zdybj.ZdyTextLayout
                app:attr_arrows="@drawable/back_right_icon"
                app:attr_key="高新企业"
                app:attr_hint="请选择"
                android:id="@+id/zel_act_czdw_gxqy"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:layout_weight="1"
            android:textColor="#4CA2FE"
            android:id="@+id/but_pop_czdw_sure"
            android:layout_gravity="center_horizontal"
            android:gravity="center"
            android:text="保存"
            android:layout_width="0dp"
            android:layout_height="35dp"/>
        <View
            android:background="#E0E0E0"
            android:layout_marginBottom="11dp"
            android:layout_marginTop="8dp"
            android:layout_width="0.5dp"
            android:layout_height="match_parent"/>
        <TextView
            android:layout_weight="1"
            android:textColor="#999999"
            android:id="@+id/but_pop_czdw_close"
            android:layout_gravity="center_horizontal"
            android:gravity="center"
            android:text="取消"
            android:layout_width="0dp"
            android:layout_height="35dp"/>

    </LinearLayout>

</LinearLayout>