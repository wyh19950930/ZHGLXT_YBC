<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    >
    <LinearLayout
        android:id="@+id/ll_item_my_title_name2"
        android:gravity="center_vertical"
        android:layout_weight="1"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="50dp">
        <View
            android:visibility="gone"
            android:id="@+id/view_item_my_title_name2"
            android:background="#12C583"
            android:layout_width="5dp"
            android:layout_height="15dp"/>
        <TextView
            android:layout_marginLeft="15.5dp"
            android:textSize="16sp"
            android:id="@+id/tv_item_my_title_name2"
            android:textColor="#333333"
            android:text="姓名"
            android:textStyle="bold"
            android:gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"/>
        <View
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"/>
<!--        android:drawableLeft="@drawable/icon_jtcytj"
android:text="添加"-->
        <TextView
            android:gravity="center_vertical"
            android:id="@+id/tv_item_my_title_name_add2"
            android:drawablePadding="5dp"
            android:layout_marginRight="14.5dp"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"/>
        <ImageView
            android:id="@+id/iv_item_my_title_name2"
            android:layout_marginRight="14dp"
            android:src="@drawable/item_title_down"
            android:layout_width="20dp"
            android:layout_height="20dp"/>
    </LinearLayout>
</LinearLayout>
