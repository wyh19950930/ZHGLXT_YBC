<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <com.jymj.zhglxt.widget.zdybj.MeItemLayout
        android:id="@+id/mil_text_list_layout1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <LinearLayout
        android:gravity="center_vertical"
        android:id="@+id/ll_text_list_layout"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <com.jymj.zhglxt.widget.zdybj.MeItemLayout
            android:id="@+id/mil_text_list_layout2"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginRight="15dp"/>
        <View
            android:background="#333333"
            android:id="@+id/view_item_text_list_layout"
            android:layout_width="1dp"
            android:layout_height="match_parent"/>
        <com.jymj.zhglxt.widget.zdybj.MeItemLayout
            android:id="@+id/mil_text_list_layout3"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="15dp"/>
    </LinearLayout>

</RelativeLayout>