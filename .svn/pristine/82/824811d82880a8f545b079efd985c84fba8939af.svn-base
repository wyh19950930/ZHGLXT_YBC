<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ll_xxcj_act_top"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f5f5f5"
    android:orientation="vertical"
    tools:context=".ldrkgl.home.activity.InformationAcquisitionActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:background="@color/white">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:gravity="center"
            android:text="信息采集"
            android:textColor="#333333"
            android:textSize="16sp"
            android:textStyle="bold"
            android:transitionName="title" />

        <ImageView
            android:id="@+id/iv_act_xxcj_back"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:padding="15dp"
            android:src="@drawable/back_left_icon" />

        <TextView
            android:visibility="gone"
            android:id="@+id/tv_act_xxcj_delete"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="47dp"
            android:padding="10dp"
            android:text="流出"
            android:textColor="@color/red"
            android:textSize="16sp"
            android:textStyle="bold" />
        <TextView
            android:visibility="gone"
            android:id="@+id/tv_act_xxcj_upload"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="7dp"
            android:padding="10dp"
            android:text="提交"
            android:textColor="#CCCCCC"
            android:textSize="16sp"
            android:textStyle="bold" />

    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="14dp"
        android:layout_marginTop="22dp"
        android:layout_marginRight="18dp"
        android:gravity="center"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="信息完成进度"
            android:textColor="#333333"
            android:textSize="14sp" />

        <LinearLayout
            android:padding="1dp"
            android:layout_centerVertical="true"
            android:background="@drawable/progressbar_actiive_quan"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="14dp"
            android:layout_weight="1">

        <ProgressBar
            android:id="@+id/xxcj_progressbar"
            android:layout_width="match_parent"
            android:layout_height="7.5dp"
            style="?android:attr/progressBarStyleHorizontal"
            android:max="100"
            android:progress="20"
            android:progressDrawable="@drawable/progressbar_color" />

        </LinearLayout>
        <TextView
            android:id="@+id/tv_xxcj_progressbar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="14dp"
            android:text="20%"
            android:textColor="#409EFF"
            android:textSize="14sp" />
    </LinearLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="22dp"
        android:layout_marginBottom="20dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <!--基本信息-->
                <RelativeLayout
                    android:id="@+id/ll_xxcj_jbxx_title"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="9dp"
                    android:background="@drawable/mapback_ffffff"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="21dp"
                        android:layout_height="22dp"
                        android:layout_marginLeft="15dp"
                        android:layout_centerVertical="true"
                        android:src="@drawable/xxcj_jbxx_icon" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="55dp"
                        android:layout_centerVertical="true"
                        android:text="基本信息"
                        android:textColor="#333333"
                        android:textSize="16sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/tv_xxcj_jbxx_tab"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="待完善"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="35dp"
                        android:textColor="#999999" />

                    <ImageView
                        android:id="@+id/iv_xxcj_jbxx_tab"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="12dp"
                        android:layout_marginRight="15dp"
                        android:src="@drawable/back_right_icon" />

                </RelativeLayout>
                <FrameLayout
                    android:visibility="gone"
                    android:clickable="true"
                    android:focusable="true"
                    android:id="@+id/ll_xxcj_jbxx_message"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="15dp"
                        android:layout_marginTop="1dp"
                        android:layout_marginRight="9dp"
                        android:background="@drawable/mapback_ffffff"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="39dp"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="12dp"
                                android:text="宅基地地址"
                                android:textColor="#333333"
                                android:textSize="14sp" />

                            <TextView
                                android:id="@+id/tv_xxcj_jbxx_zjddz"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_marginRight="12dp"
                                android:layout_weight="1"
                                android:background="@null"
                                android:gravity="right|center"
                                android:hint="宅基地地址"
                                android:textColor="#999999"
                                android:textSize="14sp" />
                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:layout_marginLeft="12dp"
                            android:layout_marginRight="12dp"
                            android:background="#eeeeee" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="39dp"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="12dp"
                                android:text="姓名"
                                android:textColor="#999999"
                                android:textSize="14sp" />

                            <EditText
                                android:id="@+id/et_xxcj_jbxx_name"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_marginRight="12dp"
                                android:layout_weight="1"
                                android:background="@null"
                                android:gravity="right|center"
                                android:hint="请输入姓名"
                                android:textColor="#333333"
                                android:textSize="14sp"
                                android:singleLine="true"/>
                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:layout_marginLeft="12dp"
                            android:layout_marginRight="12dp"
                            android:background="#eeeeee" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="39dp"
                            android:gravity="center"
                            android:orientation="horizontal"
                            android:visibility="gone">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="12dp"
                                android:text="有无身份证"
                                android:textColor="#999999"
                                android:textSize="14sp" />

                            <RadioGroup
                                android:id="@+id/rgp_xxcj_jbxx_idcard"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:gravity="right|center"
                                android:orientation="horizontal">

                                <RadioButton
                                    android:id="@+id/rbt_xxcj_jbxx_idcard_yes"
                                    android:layout_width="25dp"
                                    android:layout_height="25dp"
                                    android:background="@drawable/radiobutton_background"
                                    android:button="@null"
                                    android:checked="true"
                                    android:gravity="center"
                                    android:text="有"
                                    android:textColor="@drawable/radiobutton_textcolor"
                                    android:textSize="14sp" />

                                <RadioButton
                                    android:id="@+id/rbt_xxcj_jbxx_idcard_no"
                                    android:layout_width="25dp"
                                    android:layout_height="25dp"
                                    android:layout_marginLeft="20dp"
                                    android:layout_marginRight="12dp"
                                    android:background="@drawable/radiobutton_background"
                                    android:button="@null"
                                    android:gravity="center"
                                    android:text="无"
                                    android:textColor="@drawable/radiobutton_textcolor"
                                    android:textSize="14sp" />
                            </RadioGroup>
                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:layout_marginLeft="12dp"
                            android:layout_marginRight="12dp"
                            android:background="#eeeeee"
                            android:visibility="gone"/>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="39dp"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="12dp"
                                android:text="身份证号码"
                                android:textColor="#999999"
                                android:textSize="14sp" />
                            <com.google.android.material.textfield.TextInputLayout
                                android:id="@+id/etil_xxcj_jbxx_sfzhm"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                app:hintEnabled="false"
                                >
                                <EditText
                                    android:id="@+id/et_xxcj_jbxx_sfzhm"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginRight="12dp"
                                    android:background="@null"
                                    android:hint="请输入身份证号码"
                                    android:gravity="right|center"
                                    android:textColor="#333333"
                                    android:textSize="14sp"
                                    android:maxLength="18"
                                    android:digits="0123456789qwertyuioplkjhgfdsazxcvbnm"
                                    android:singleLine="true"/>
                            </com.google.android.material.textfield.TextInputLayout>

                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:layout_marginLeft="12dp"
                            android:layout_marginRight="12dp"
                            android:background="#eeeeee" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="39dp"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="12dp"
                                android:text="性别"
                                android:textColor="#999999"
                                android:textSize="14sp" />

                            <RadioGroup
                                android:id="@+id/rgp_xxcj_jbxx_sex"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:gravity="right|center"
                                android:orientation="horizontal">

                                <RadioButton
                                    android:id="@+id/rbt_xxcj_jbxx_sex_man"
                                    android:layout_width="25dp"
                                    android:layout_height="25dp"
                                    android:background="@drawable/radiobutton_background"
                                    android:button="@null"
                                    android:checked="true"
                                    android:gravity="center"
                                    android:text="男"
                                    android:textColor="@drawable/radiobutton_textcolor"
                                    android:textSize="14sp" />

                                <RadioButton
                                    android:id="@+id/rbt_xxcj_jbxx_sex_woman"
                                    android:layout_width="25dp"
                                    android:layout_height="25dp"
                                    android:layout_marginLeft="20dp"
                                    android:layout_marginRight="12dp"
                                    android:background="@drawable/radiobutton_background"
                                    android:button="@null"
                                    android:gravity="center"
                                    android:text="女"
                                    android:textColor="@drawable/radiobutton_textcolor"
                                    android:textSize="14sp" />
                            </RadioGroup>
                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:layout_marginLeft="12dp"
                            android:layout_marginRight="12dp"
                            android:background="#eeeeee" />

                        <LinearLayout
                            android:id="@+id/ll_xxcj_csrq"
                            android:layout_width="match_parent"
                            android:layout_height="39dp"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_weight="3"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="12dp"
                                android:text="出生日期"
                                android:textColor="#999999"
                                android:textSize="14sp" />

                            <LinearLayout
                                android:layout_marginRight="12dp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="2"
                                android:gravity="right"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/et_xxcj_csrq_year"
                                    android:layout_weight="1"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:gravity="right"
                                    android:background="@null"
                                    android:text="年"
                                    android:textColor="#999999"
                                    android:textSize="14sp" />

                                <TextView
                                    android:id="@+id/et_xxcj_csrq_mouth"
                                    android:layout_weight="1"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:gravity="right"
                                    android:background="@null"
                                    android:text="月"
                                    android:textColor="#999999"
                                    android:textSize="14sp" />

                                <TextView
                                    android:id="@+id/et_xxcj_csrq_day"
                                    android:layout_weight="1"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:gravity="right"
                                    android:background="@null"
                                    android:text="日"
                                    android:textColor="#999999"
                                    android:textSize="14sp" />

                            </LinearLayout>
                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:layout_marginLeft="12dp"
                            android:layout_marginRight="12dp"
                            android:background="#eeeeee" />

                        <LinearLayout
                            android:id="@+id/ll_xxcj_zzmm"
                            android:layout_width="match_parent"
                            android:layout_height="39dp"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="12dp"
                                android:text="政治面貌"
                                android:textColor="#999999"
                                android:textSize="14sp" />
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="right"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/tv_xxcj_zzmm"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginRight="12dp"
                                    android:background="@null"
                                    android:text="请选择"
                                    android:textColor="#999999"
                                    android:textSize="14sp" />

                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginRight="15dp"
                                    android:src="@drawable/back_right_icon" />
                            </LinearLayout>
                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:layout_marginLeft="12dp"
                            android:layout_marginRight="12dp"
                            android:background="#eeeeee" />

                        <LinearLayout
                            android:id="@+id/ll_xxcj_jycd"
                            android:layout_width="match_parent"
                            android:layout_height="39dp"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="12dp"
                                android:text="教育程度"
                                android:textColor="#999999"
                                android:textSize="14sp" />

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="right"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/tv_xxcj_jycd"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginRight="12dp"
                                    android:background="@null"
                                    android:text="请选择"
                                    android:textColor="#999999"
                                    android:textSize="14sp" />

                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginRight="15dp"
                                    android:src="@drawable/back_right_icon" />
                            </LinearLayout>
                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:layout_marginLeft="12dp"
                            android:layout_marginRight="12dp"
                            android:background="#eeeeee" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="39dp"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="12dp"
                                android:text="户籍类别"
                                android:textColor="#999999"
                                android:textSize="14sp" />

                            <RadioGroup
                                android:id="@+id/rgp_xxcj_jbxx_hjlb"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:gravity="right|center"
                                android:orientation="horizontal">

                                <RadioButton
                                    android:id="@+id/rbt_xxcj_jbxx_hjlb_n"
                                    android:layout_width="70dp"
                                    android:layout_height="25dp"
                                    android:background="@drawable/radiobutton_background"
                                    android:button="@null"
                                    android:checked="true"
                                    android:gravity="center"
                                    android:text="农业户口"
                                    android:textColor="@drawable/radiobutton_textcolor"
                                    android:textSize="14sp" />

                                <RadioButton
                                    android:id="@+id/rbt_xxcj_jbxx_hjlb_fn"
                                    android:layout_width="70dp"
                                    android:layout_height="25dp"
                                    android:layout_marginLeft="20dp"
                                    android:layout_marginRight="12dp"
                                    android:background="@drawable/radiobutton_background"
                                    android:button="@null"
                                    android:gravity="center"
                                    android:text="非农户口"
                                    android:textColor="@drawable/radiobutton_textcolor"
                                    android:textSize="14sp" />

                                <RadioButton
                                    android:id="@+id/rbt_xxcj_jbxx_hjlb_whk"
                                    android:layout_width="70dp"
                                    android:layout_height="25dp"
                                    android:layout_marginRight="12dp"
                                    android:background="@drawable/radiobutton_background"
                                    android:button="@null"
                                    android:gravity="center"
                                    android:text="无户口"
                                    android:textColor="@drawable/radiobutton_textcolor"
                                    android:textSize="14sp" />
                            </RadioGroup>
                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:layout_marginLeft="12dp"
                            android:layout_marginRight="12dp"
                            android:background="#eeeeee" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="39dp"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="12dp"
                                android:text="婚姻情况"
                                android:textColor="#999999"
                                android:textSize="14sp" />

                            <RadioGroup
                                android:id="@+id/rgp_xxcj_jbxx_hyzk"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:gravity="right|center"
                                android:orientation="horizontal">

                                <RadioButton
                                    android:id="@+id/rbt_xxcj_jbxx_hyzk_wh"
                                    android:layout_width="40dp"
                                    android:layout_height="25dp"
                                    android:background="@drawable/radiobutton_background"
                                    android:button="@null"
                                    android:checked="true"
                                    android:gravity="center"
                                    android:text="未婚"
                                    android:textColor="@drawable/radiobutton_textcolor"
                                    android:textSize="14sp" />

                                <RadioButton
                                    android:id="@+id/rbt_xxcj_jbxx_hyzk_yh"
                                    android:layout_width="40dp"
                                    android:layout_height="25dp"
                                    android:layout_marginLeft="20dp"
                                    android:background="@drawable/radiobutton_background"
                                    android:button="@null"
                                    android:gravity="center"
                                    android:text="已婚"
                                    android:textColor="@drawable/radiobutton_textcolor"
                                    android:textSize="14sp" />

                                <RadioButton
                                    android:id="@+id/rbt_xxcj_jbxx_hyzk_lh"
                                    android:layout_width="40dp"
                                    android:layout_height="25dp"
                                    android:layout_marginLeft="20dp"
                                    android:background="@drawable/radiobutton_background"
                                    android:button="@null"
                                    android:gravity="center"
                                    android:text="离婚"
                                    android:textColor="@drawable/radiobutton_textcolor"
                                    android:textSize="14sp" />

                                <RadioButton
                                    android:id="@+id/rbt_xxcj_jbxx_hyzk_so"
                                    android:layout_width="40dp"
                                    android:layout_height="25dp"
                                    android:layout_marginLeft="20dp"
                                    android:layout_marginRight="12dp"
                                    android:background="@drawable/radiobutton_background"
                                    android:button="@null"
                                    android:gravity="center"
                                    android:text="丧偶"
                                    android:textColor="@drawable/radiobutton_textcolor"
                                    android:textSize="14sp" />
                            </RadioGroup>
                        </LinearLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:layout_marginLeft="12dp"
                            android:layout_marginRight="12dp"
                            android:background="#eeeeee" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="39dp"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="12dp"
                                android:text="联系电话"
                                android:textColor="#999999"
                                android:textSize="14sp" />
                            <com.google.android.material.textfield.TextInputLayout
                                android:id="@+id/etil_xxcj_jbxx_phone"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                app:hintEnabled="false"
                                >
                                <EditText
                                    android:id="@+id/et_xxcj_jbxx_phone"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginRight="12dp"
                                    android:layout_weight="1"
