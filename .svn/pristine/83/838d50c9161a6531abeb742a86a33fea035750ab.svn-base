<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:id="@+id/ll_act_czdw_detail"
    tools:context="com.jymj.zhglxt.zjd.activity.CzdwDetailActivity">
    <RelativeLayout
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="50dp">
        <ImageView
            android:id="@+id/iv_act_czdw_detail_back"
            android:layout_marginLeft="10dp"
            android:layout_centerVertical="true"
            android:src="@drawable/back_left_icon"
            android:layout_width="25dp"
            android:layout_height="25dp"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="承租单位详情"
            android:textColor="@color/black"
            android:textSize="18sp"
            android:textStyle="bold" />
        <TextView
            android:id="@+id/tv_act_czdw_detail_delete"
            android:layout_marginRight="10dp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:textColor="@color/red"
            android:text="删除"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </RelativeLayout>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:id="@+id/ll_act_czdw_detail_qyxx"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <com.jymj.zhglxt.widget.zdybj.MeTitleLayout
                app:attr_leftname="修改"
                app:attr_name="注册情况"
                app:attr_color="#4CA2FE"
                android:id="@+id/mtl_act_czdw_detail_zcqk"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>
            <LinearLayout
                android:layout_marginRight="13.5dp"
                android:layout_marginLeft="16dp"
                android:id="@+id/ll_act_czdw_detail_zcqk"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
<!--                ZdyEditLayout-->
                <com.jymj.zhglxt.widget.zdybj.ZdyTextLayout
                    app:attr_key="现状企业名称"
                    app:attr_hint="请输入现状企业名称"
                    android:id="@+id/zel_act_czdw_detail_xzqymc1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
                <com.jymj.zhglxt.widget.zdybj.ZdyTextLayout
                    app:attr_key="注册地址"
                    app:attr_hint="请输入注册地址"
                    android:id="@+id/zel_act_czdw_detail_zcqk"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
                <com.jymj.zhglxt.widget.zdybj.ZdyTextLayout
                    app:attr_key="实际经营地址"
                    app:attr_hint="请输入实际经营地址"
                    android:id="@+id/zel_act_czdw_detail_sjjydz"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
                <com.jymj.zhglxt.widget.zdybj.ZdyTextLayout
                    app:attr_key="入驻日期"
                    app:attr_hint="请选择时间"
                    android:id="@+id/ztl_act_czdw_detail_rzrq"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
                <com.jymj.zhglxt.widget.zdybj.ZdyTextLayout
                    app:attr_key="营业执照注册日期"
                    app:attr_hint="请选择注册日期"
                    android:id="@+id/ztl_act_czdw_detail_yyzzzcrq"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
                <com.jymj.zhglxt.widget.zdybj.ZdyTextLayout
                    app:attr_key="税务登记注册地"
                    app:attr_hint="请输入税务登记注册地"
                    android:id="@+id/zel_act_czdw_detail_swdjzcd"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
                <View
                    android:background="@color/gray"
                    android:layout_width="match_parent"
                    android:layout_height="10dp"/>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content">

<!--                        app:attr_arrows="@drawable/back_right_icon"-->
                        <com.jymj.zhglxt.widget.zdybj.ZdyTextLayout
                            app:attr_key="企业类型"
                            app:attr_hint="请选择企业类型"
                            android:id="@+id/ztl_act_czdw_detail_qylx"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"/>
<!--                        app:attr_arrows="@drawable/back_right_icon"-->
                        <com.jymj.zhglxt.widget.zdybj.ZdyTextLayout
                            app:attr_key="行业类型"
                            app:attr_hint="请选择行业类型"
                            android:id="@+id/ztl_act_czdw_detail_hylx"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"/>
                        <com.jymj.zhglxt.widget.zdybj.ZdyTextLayout
                            app:attr_key="行政联系人"
                            app:attr_hint="请输入行政联系人"
                            android:id="@+id/zel_act_czdw_detail_xzlxr"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"/>
<!--                        app:attr_arrows="@drawable/back_right_icon"-->
                        <com.jymj.zhglxt.widget.zdybj.ZdyTextLayout
                            app:attr_key="是否转租"
                            app:attr_hint="请选择是否转租"
                            android:id="@+id/ztl_act_czdw_detail_sfzz"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"/>
                    </LinearLayout>
                    <View
                        android:layout_marginRight="5dp"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="10dp"
                        android:background="#333333"
                        android:layout_width="1dp"
                        android:layout_height="match_parent"/>
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content">

<!--                        app:attr_arrows="@drawable/back_right_icon"-->
                        <com.jymj.zhglxt.widget.zdybj.ZdyTextLayout
                            app:attr_key="注册类型"
                            app:attr_hint="请选择注册类型"
                            android:id="@+id/ztl_act_czdw_detail_zclx"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"/>
                        <com.jymj.zhglxt.widget.zdybj.ZdyTextLayout
                            app:attr_key="注册资金"
                            app:attr_hint="请输入注册资金"
                            android:id="@+id/zel_act_czdw_detail_zczj"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"/>
                        <com.jymj.zhglxt.widget.zdybj.ZdyTextLayout
                            app:attr_key="联系电话"
                            app:attr_hint="请输入联系电话"
                            android:id="@+id/zel_act_czdw_detail_lxdh"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"/>
<!--                        app:attr_arrows="@drawable/back_right_icon"-->
                        <com.jymj.zhglxt.widget.zdybj.ZdyTextLayout
                            app:attr_key="高新企业"
                            app:attr_hint="请选择高新企业"
                            android:id="@+id/ztl_act_czdw_detail_gxqy"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"/>
                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:background="#F4F1F1"
                android:gravity="center_vertical"
                android:layout_weight="1"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="38.5dp">
                <View
                    android:id="@+id/view_item_my_title_name"
                    android:background="#4CA2FE"
                    android:layout_width="5dp"
                    android:layout_height="15dp"/>
                <TextView
                    android:layout_marginLeft="15.5dp"
                    android:textSize="16sp"
                    android:textColor="#333333"
                    android:text="经营情况"
                    android:gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"/>
                <View
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"/>
                <TextView
                    android:id="@+id/but_act_czdw_detail_add"
                    android:layout_marginRight="10dp"
                    android:textColor="#4CA2FE"
                    android:text="添加"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
                <TextView
                    android:hint="请选择时间"
                    android:textColor="#4CA2FE"
                    android:gravity="center_vertical"
                    android:id="@+id/tv_act_czdw_detail_time"
                    android:drawablePadding="5dp"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"/>
                <ImageView
                    android:layout_marginRight="14.5dp"
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="3dp"
                    android:layout_marginTop="3dp"
                    android:background="@drawable/inverted_triangle_style_blue" />
            </LinearLayout>
            <LinearLayout
                android:layout_marginRight="13.5dp"
                android:layout_marginLeft="16dp"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content">

                        <com.jymj.zhglxt.widget.zdybj.ZdyTextLayout
                            app:attr_key="利润总额"
                            app:attr_hint="请输入利润总额"
                            android:id="@+id/zel_act_czdw_detail_lrze"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"/>
                        <com.jymj.zhglxt.widget.zdybj.ZdyTextLayout
                            app:attr_key="税收"
                            app:attr_hint="请输入税收"
                            android:id="@+id/zel_act_czdw_detail_ss"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"/>
                    </LinearLayout>
                    <View
                        android:layout_marginRight="5dp"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="10dp"
                        android:background="#333333"
                        android:layout_width="1dp"
                        android:layout_height="match_parent"/>
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content">

                        <com.jymj.zhglxt.widget.zdybj.ZdyTextLayout
                            app:attr_key="营业收入"
                            app:attr_hint="请输入营业收入"
                            android:id="@+id/zel_act_czdw_detail_yysr"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"/>
                        <com.jymj.zhglxt.widget.zdybj.ZdyTextLayout
                            app:attr_key="产值"
                            app:attr_hint="请输入产值"
                            android:id="@+id/zel_act_czdw_detail_cz"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"/>
                    </LinearLayout>

                </LinearLayout>
                <com.jymj.zhglxt.widget.zdybj.ZdyTextLayout
                    app:attr_key="经营状态"
                    app:attr_hint="经营状态"
                    android:id="@+id/ztl_act_czdw_detail_jyzt"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
                <com.jymj.zhglxt.widget.zdybj.ZdyTextLayout
                    app:attr_key="本地员工数量"
                    app:attr_hint="本地员工数量"
                    android:id="@+id/ztl_act_czdw_detail_bdygsl"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
                <com.jymj.zhglxt.widget.zdybj.ZdyTextLayout
                    app:attr_key="外地员工数量"
                    app:attr_hint="外地员工数量"
                    android:id="@+id/ztl_act_czdw_detail_wdygsl"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
                <com.jymj.zhglxt.widget.zdybj.ZdyTextLayout
                    app:attr_key="就业人口居住区域(单位宿舍)"
                    app:attr_hint="就业人口居住区域"
                    android:id="@+id/ztl_act_czdw_detail_jyrkjzqy1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
                <com.jymj.zhglxt.widget.zdybj.ZdyTextLayout
                    app:attr_key="就业人口居住区域(本村)"
                    app:attr_hint="就业人口居住区域"
                    android:id="@+id/ztl_act_czdw_detail_jyrkjzqy2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>
                <com.jymj.zhglxt.widget.zdybj.ZdyTextLayout
                    app:attr_key="就业人口居住区域(其他)"
                    app:attr_hint="就业人口居住区域"
                    android:id="@+id/ztl_act_czdw_detail_jyrkjzqy3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"/>

            </LinearLayout>
            <!--<Button
                android:layout_marginBottom="33dp"
                android:id="@+id/but_act_czdw_detail_add"
                android:background="@drawable/bt_actiive_kuang4ca2fe"
                android:layout_marginRight="13.5dp"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="11.5dp"
                android:textColor="#4CA2FE"
                android:text="添加"
                android:layout_width="match_parent"
                android:layout_height="39dp"/>-->

        </LinearLayout>
    </ScrollView>


</LinearLayout>
