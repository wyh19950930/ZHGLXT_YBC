<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@drawable/bt_actiive_fef2dc"
    android:padding="5dp"
    android:id="@+id/tv_pop_first_map1"
    android:orientation="vertical">
    <TextView
        android:id="@+id/tv_pop_first_map1_name"
        android:textSize="16sp"
        android:textColor="#f19200"
        android:text="张辛庄村"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <TextView
        android:textSize="13sp"
        android:id="@+id/tv_pop_first_map1_all"
        android:layout_marginTop="2dp"
        android:textColor="#c79fe3"
        android:text="全部102"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <LinearLayout
        android:layout_marginTop="1dp"
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <TextView
            android:textSize="13sp"
            android:id="@+id/tv_pop_first_map1_yz"
            android:textColor="#c79fe3"
            android:text="严重12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <View
            android:layout_width="10dp"
            android:layout_height="1dp"/>
        <TextView
            android:textStyle="bold"
            android:id="@+id/tv_pop_first_map1_zb"
            android:textSize="16sp"
            android:textColor="#f19200"
            android:text="75%"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </LinearLayout>
    <!--<TextView
           android:padding="8dp"
           android:gravity="center"
           android:textColor="@color/white"
           android:background="@drawable/arrow_center"
           android:id="@+id/tv_pop_first_map"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"/>-->
</LinearLayout>