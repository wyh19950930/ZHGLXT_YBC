<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/ll_frag_zjd_cyyq_top"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white"
    tools:context="com.jymj.zhglxt.zjd.fragment.ZjdYdFragment">

    <LinearLayout
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:orientation="horizontal"
        android:gravity="center_vertical"
        >
        <View
            android:layout_width="5dp"
            android:layout_height="15dp"
            android:background="#4CA2FE"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="条件查询"
            android:textColor="#333333"
            android:textSize="16sp"
            android:textStyle="bold"
            android:layout_marginLeft="10dp"
            />

    </LinearLayout>
    <LinearLayout
        android:gravity="center_vertical"
        android:background="#4CA2FE"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="42dp">
        <LinearLayout
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:gravity="center_vertical"
            android:background="@drawable/mapback_ffffff2"
            android:layout_weight="1"
            android:layout_marginLeft="15dp"
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="30dp">
            <ImageView
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="12dp"
                android:src="@drawable/hssearch_czlb"
                android:layout_width="15dp"
                android:layout_height="15dp"/>
            <com.jymj.zhglxt.widget.ClearEditText
                android:layout_marginRight="10dp"
                android:id="@+id/et_act_qygl_detail_qymc"
                android:layout_marginLeft="5dp"
                android:background="@null"
                android:hint="请输入企业名称"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textColor="#999999"
                android:textSize="14sp"
                android:maxLength="9"
                />

        </LinearLayout>
        <TextView
            android:id="@+id/tv_act_qygl_detail_search"
            android:padding="11dp"
            android:textColor="@color/white"
            android:text="搜索"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:orientation="horizontal">
        <LinearLayout
            android:id="@+id/ll_act_qygl_detail_qylx"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:gravity="center">
            <TextView
                android:id="@+id/et_act_qygl_detail_qylx"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center"
                android:hint="请选择行业类型"
                android:textColor="#666666"
                android:textSize="14sp"/>
            <ImageView
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_gravity="center"
                android:layout_marginLeft="3dp"
                android:layout_marginTop="3dp"
                android:background="@drawable/inverted_triangle_style_blue" />
<!--            inverted_triangle_style_grey-->
        </LinearLayout>
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#cccccc"/>
        <LinearLayout
            android:id="@+id/ll_act_qygl_detail_qzcsj"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:gravity="center">

            <TextView
                android:id="@+id/et_act_qygl_detail_qzcsj"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center"
                android:hint="请选择经营状态"
                android:textColor="#666666"
                android:textSize="14sp" />

            <ImageView
                android:layout_width="20dp"
                android:layout_height="15dp"
                android:layout_marginTop="5dp"
                android:layout_marginRight="8.5dp"
                android:background="@drawable/inverted_triangle_style_blue" />
        </LinearLayout>
        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="#cccccc"/>
        <ImageView
            android:id="@+id/tv_act_qygl_detail_reset"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/reset_option_icon"
            android:layout_gravity="center"
            android:padding="11dp"/>

    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#cccccc"/>

    <LinearLayout
        android:visibility="gone"
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal"
        android:gravity="center_vertical"
        >
        <View
            android:layout_width="5dp"
            android:layout_height="15dp"
            android:background="#4CA2FE"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="行业类型"
            android:textColor="#333333"
            android:textSize="16sp"
            android:textStyle="bold"
            android:layout_marginLeft="10dp"
            />

    </LinearLayout>
    <View
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="#333333"/>

    <com.github.mikephil.charting.charts.BarChart
        android:visibility="gone"
        android:id="@+id/bct_frag_cyyq"
        android:layout_width="match_parent"
        android:layout_height="180dp"/>

    <androidx.recyclerview.widget.RecyclerView
        android:layout_gravity="center_horizontal"
        android:id="@+id/rlv_frag_zjd_cyyq_hylx"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <LinearLayout
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal"
        android:gravity="center_vertical"
        >
        <View
            android:layout_width="5dp"
            android:layout_height="15dp"
            android:background="#4CA2FE"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="列表详情"
            android:textColor="#333333"
            android:textSize="16sp"
            android:textStyle="bold"
            android:layout_marginLeft="10dp"
            />

    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="#333333"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:orientation="horizontal"
        android:gravity="center">
        <TextView
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:paddingLeft="14dp"
            android:text="编号"
            android:textColor="#333333"
            android:textSize="11sp"
            android:gravity="left"/>
        <TextView
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="企业名称"
            android:textColor="#333333"
            android:textSize="11sp"
            android:gravity="center"/>
<!--        地址-->
        <TextView
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="权属"
            android:textColor="#333333"
            android:textSize="11sp"
            android:gravity="center"/>
        <TextView
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="占地面积"
            android:textColor="#333333"
            android:textSize="11sp"
            android:gravity="center"/>
        <View
            android:layout_width="30dp"
            android:layout_height="wrap_content"/>
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1.5dp"
        android:background="#666666"/>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <!--android:focusable="true"
        android:focusableInTouchMode="true"-->
<!--        OptimizedRecyclerViewZhen-->
    <com.jymj.zhglxt.widget.OptimizedRecyclerViewZhen
        android:id="@+id/rlv_act_qygl_detail"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        
    </com.jymj.zhglxt.widget.OptimizedRecyclerViewZhen>

    </RelativeLayout>
</LinearLayout>
