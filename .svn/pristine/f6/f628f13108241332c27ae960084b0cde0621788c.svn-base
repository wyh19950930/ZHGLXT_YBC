<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:id="@+id/ll_act_me"
    tools:context=".ldrkgl.personal.activity.MeActivity">
    <RelativeLayout
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="40dp">
        <ImageView
            android:visibility="gone"
            android:paddingBottom="10dp"
            android:paddingTop="10dp"
            android:paddingRight="10dp"
            android:layout_centerVertical="true"
            android:id="@+id/iv_act_me_back"
            android:src="@drawable/back_left_icon"
            android:layout_gravity="center_vertical"
            android:paddingLeft="12dp"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"/>
        <TextView
            android:layout_centerInParent="true"
            android:textColor="#333333"
            android:textSize="18sp"
            android:layout_marginLeft="7dp"
            android:layout_gravity="center_vertical"
            android:text="我的"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </RelativeLayout>

    <LinearLayout
        android:background="@color/white"
        android:layout_marginTop="10dp"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:background="@drawable/ripple_bg"
            android:id="@+id/rl_act_me_personal_data"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <ImageView
                android:id="@+id/iv_act_me_head"
                android:src="@drawable/head_portrait_home"
                android:layout_margin="10dp"
                android:layout_width="50dp"
                android:layout_height="50dp"/>
            <TextView
                android:layout_marginTop="22dp"
                android:layout_toRightOf="@id/iv_act_me_head"
                android:textSize="15sp"
                android:textColor="#333333"
                android:text="姓名"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <TextView
                android:layout_marginTop="45dp"
                android:layout_toRightOf="@id/iv_act_me_head"
                android:textSize="12sp"
                android:textColor="#333333"
                android:text="158****6038"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

            <TextView
                android:layout_marginTop="45dp"
                android:textSize="13sp"
                android:textColor="#999999"
                android:text="编辑资料"
                android:layout_alignParentRight="true"
                android:layout_marginRight="40dp"
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

            <ImageView
                android:layout_marginRight="15dp"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:src="@drawable/back_right_icon"
                android:layout_width="15dp"
                android:layout_height="15dp"/>
        </RelativeLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="10dp"
            android:paddingBottom="10dp"
            android:orientation="horizontal">

            <TextView
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="我的发布"
                android:textColor="#666666"
                android:textSize="15sp"
                android:gravity="center"
                android:drawableTop="@drawable/me_wdfb_icon"
                android:drawablePadding="3dp"
                />
            <TextView
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="我的参与"
                android:textColor="#666666"
                android:textSize="15sp"
                android:gravity="center"
                android:drawableTop="@drawable/me_wdcy_icon"
                android:drawablePadding="3dp"
                />
            <TextView
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="我的收藏"
                android:textColor="#666666"
                android:textSize="15sp"
                android:gravity="center"
                android:drawableTop="@drawable/me_wdsc_icon"
                android:drawablePadding="3dp"
                />
        </LinearLayout>

        <View
            android:background="#fafafa"
            android:layout_width="match_parent"
            android:layout_height="10dp"/>
        <LinearLayout
            android:visibility="gone"
            android:id="@+id/ll_act_me_sczq"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="商城专区"
                android:textSize="16sp"
                android:textColor="#333333"
                android:textStyle="bold"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="5dp"
                />
            <ImageView
                android:id="@+id/iv_act_me_shoppingmall"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/shopping_mall_banner"
                android:layout_gravity="center"
                />
            <View
                android:background="#f6f7fb"
                android:layout_width="match_parent"
                android:layout_height="10dp"
                android:layout_marginTop="10dp"/>
        </LinearLayout>


        <LinearLayout
            android:id="@+id/ll_act_me_mmaq"
            android:background="@drawable/ripple_bg"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="45dp">
            <ImageView
                android:layout_marginLeft="15dp"
                android:src="@drawable/mmaq_me"
                android:layout_width="20dp"
                android:layout_height="20dp"/>
            <TextView
                android:layout_weight="1"
                android:layout_marginLeft="10dp"
                android:textColor="#333333"
                android:textSize="15sp"
                android:text="密码安全"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <ImageView
                android:layout_marginRight="15dp"
                android:src="@drawable/back_right_icon"
                android:layout_width="15dp"
                android:layout_height="15dp"/>
        </LinearLayout>
        <!--<View
            android:background="@color/gray"
            android:layout_width="match_parent"
            android:layout_height="1dp"/>-->
        <LinearLayout
            android:id="@+id/ll_act_me_qhzh"
            android:background="@drawable/ripple_bg"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="45dp">
            <ImageView
                android:layout_marginLeft="15dp"
                android:src="@drawable/mmaq_me"
                android:layout_width="20dp"
                android:layout_height="20dp"/>
            <TextView
                android:layout_weight="1"
                android:layout_marginLeft="10dp"
                android:textColor="#333333"
                android:textSize="15sp"
                android:text="切换账号"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <ImageView
                android:layout_marginRight="15dp"
                android:src="@drawable/back_right_icon"
                android:layout_width="15dp"
                android:layout_height="15dp"/>
        </LinearLayout>

    </LinearLayout>
    <LinearLayout
        android:layout_marginTop="10dp"
        android:id="@+id/ll_act_me_wdxx"
        android:background="@drawable/ripple_bg"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="45dp">
        <ImageView
            android:layout_marginLeft="15dp"
            android:src="@drawable/wdxx_me"
            android:layout_width="20dp"
            android:layout_height="20dp"/>
        <TextView
            android:layout_weight="1"
            android:layout_marginLeft="10dp"
            android:textColor="#333333"
            android:textSize="15sp"
            android:text="我的消息"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/tv_act_me_wcls"
            android:layout_marginRight="14dp"
            android:textColor="@color/red"
            android:text=""
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <ImageView
            android:layout_marginRight="15dp"
            android:src="@drawable/back_right_icon"
            android:layout_width="15dp"
            android:layout_height="15dp"/>
    </LinearLayout>
    <LinearLayout
        android:layout_marginTop="10dp"
        android:id="@+id/ll_act_me_yhlb"
        android:background="@drawable/ripple_bg"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="45dp">
        <ImageView
            android:layout_marginLeft="15dp"
            android:src="@drawable/userinfo"
            android:layout_width="25dp"
            android:layout_height="25dp"/>
        <TextView
            android:layout_weight="1"
            android:layout_marginLeft="10dp"
            android:textColor="#333333"
            android:textSize="15sp"
            android:text="用户列表"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <ImageView
            android:layout_marginRight="15dp"
            android:src="@drawable/back_right_icon"
            android:layout_width="15dp"
            android:layout_height="15dp"/>
    </LinearLayout>
    <LinearLayout
        android:layout_marginTop="10dp"
        android:id="@+id/ll_act_me_jcgx"
        android:background="@drawable/ripple_bg"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="45dp">
        <ImageView
            android:layout_marginLeft="15dp"
            android:src="@drawable/jcgx_me"
            android:layout_width="20dp"
            android:layout_height="20dp"/>
        <TextView
            android:layout_weight="1"
            android:layout_marginLeft="10dp"
            android:textColor="#333333"
            android:textSize="15sp"
            android:text="检查更新"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/tv_act_me_bbh"
            android:layout_marginRight="14dp"
            android:text="v 1.0版本"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <ImageView
            android:layout_marginRight="15dp"
            android:src="@drawable/back_right_icon"
            android:layout_width="15dp"
            android:layout_height="15dp"/>
    </LinearLayout>
    <View
        android:background="@color/gray"
        android:layout_width="match_parent"
        android:layout_height="1dp"/>
    <LinearLayout
        android:id="@+id/ll_act_me_qchc"
        android:background="@drawable/ripple_bg"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="45dp">
        <ImageView
            android:layout_marginLeft="15dp"
            android:src="@drawable/qlhc_me"
            android:layout_width="20dp"
            android:layout_height="20dp"/>
        <TextView
            android:layout_weight="1"
            android:layout_marginLeft="10dp"
            android:textColor="#333333"
            android:textSize="15sp"
            android:text="清除缓存"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/tv_act_me_size"
            android:layout_marginRight="14dp"
            android:text="4.5 MB"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <ImageView
            android:layout_marginRight="15dp"
            android:src="@drawable/back_right_icon"
            android:layout_width="15dp"
            android:layout_height="15dp"/>
    </LinearLayout>
    <TextView
        android:id="@+id/but_act_me_out"
        android:gravity="center"
        android:textColor="@color/red"
        android:layout_marginTop="10dp"
        android:background="@color/white"
        android:textSize="20sp"
        android:text="退出系统"
        android:layout_width="match_parent"
        android:layout_height="45dp"/>



</LinearLayout>