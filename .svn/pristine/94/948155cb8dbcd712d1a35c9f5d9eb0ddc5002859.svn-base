<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white"
    tools:context="com.jymj.zhglxt.zjd.fragment.ZjdYdFragment">

    <ScrollView
        android:id="@+id/slv_frag_zjd_wy"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <LinearLayout
                android:background="@color/white"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                >
                <View
                    android:layout_width="5dp"
                    android:layout_height="15dp"
                    android:background="#12C583"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="社区管理统计"
                    android:textColor="#333333"
                    android:textSize="@dimen/sp_16"
                    android:textStyle="bold"
                    android:layout_marginLeft="10dp"
                    />


            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="#333333"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <RelativeLayout
                        android:id="@+id/aaaa"
                        android:layout_weight="2"
                        android:layout_width="0dp"
                        android:layout_height="135dp"
                        android:background="@drawable/title_chage_wyhgl">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/wy_wyhgl_icon"
                            android:layout_marginTop="10dp"
                            android:layout_marginLeft="10dp"
                            />

                        <TextView
                            android:id="@+id/tv_frag_zjd_cz_wyhgl"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:text="xx"
                            android:textSize="24sp"
                            android:textStyle="bold"
                            android:textColor="@color/white"
                            android:layout_marginLeft="10dp"
                            android:layout_marginBottom="23.5dp"
                            />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:text="物业化管理（村）"
                            android:textSize="14sp"
                            android:textColor="@color/white"
                            android:layout_marginLeft="10dp"
                            android:layout_marginBottom="5.5dp"
                            />

                    </RelativeLayout>

                    <LinearLayout
                        android:layout_weight="3"
                        android:layout_width="0dp"
                        android:layout_height="135dp"
                        android:orientation="vertical"
                        android:layout_marginLeft="5dp">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="65dp"
                            android:background="@drawable/title_chage_ylzs">

                            <TextView
                                android:id="@+id/tv_frag_zjd_cz_ylzs"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentBottom="true"
                                android:text="xx"
                                android:textSize="24sp"
                                android:textStyle="bold"
                                android:textColor="@color/white"
                                android:layout_marginLeft="10dp"
                                android:layout_marginBottom="24.5dp"
                                />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentBottom="true"
                                android:text="院落总数（宗）"
                                android:textSize="14sp"
                                android:textColor="@color/white"
                                android:layout_marginLeft="10dp"
                                android:layout_marginBottom="6.5dp"
                                />

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:src="@drawable/wy_ylzs_icon"
                                android:layout_marginTop="13dp"
                                android:layout_marginRight="15dp"
                                android:layout_alignParentRight="true"
                                />

                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="65dp"
                            android:layout_marginTop="5dp"
                            android:background="@drawable/title_chage_fjs">

                            <TextView
                                android:id="@+id/tv_frag_zjd_cz_fjs"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentBottom="true"
                                android:text="xx"
                                android:textSize="24sp"
                                android:textStyle="bold"
                                android:textColor="@color/white"
                                android:layout_marginLeft="10dp"
                                android:layout_marginBottom="24.5dp"
                                />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentBottom="true"
                                android:text="房间数（间）"
                                android:textSize="14sp"
                                android:textColor="@color/white"
                                android:layout_marginLeft="10dp"
                                android:layout_marginBottom="6.5dp"
                                />

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:src="@drawable/wy_fjs_icon"
                                android:layout_marginTop="13dp"
                                android:layout_marginRight="21.5dp"
                                android:layout_alignParentRight="true"
                                />

                        </RelativeLayout>

                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"
                    android:orientation="horizontal">

                    <RelativeLayout
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:layout_height="65dp"
                        android:background="@drawable/title_chage_yczf">

                        <TextView
                            android:id="@+id/tv_frag_zjd_cz_yczf"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:text="xx"
                            android:textSize="24sp"
                            android:textStyle="bold"
                            android:textColor="@color/white"
                            android:layout_marginLeft="10dp"
                            android:layout_marginBottom="24.5dp"
                            />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:text="已出租房（间）"
                            android:textSize="14sp"
                            android:textColor="@color/white"
                            android:layout_marginLeft="10dp"
                            android:layout_marginBottom="6.5dp"
                            />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/wy_yczf_icon"
                            android:layout_marginTop="13dp"
                            android:layout_marginRight="15dp"
                            android:layout_alignParentRight="true"
                            />

                    </RelativeLayout>
                    <RelativeLayout
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:layout_height="65dp"
                        android:layout_marginLeft="5dp"
                        android:background="@drawable/title_chage_zlrk">

                        <TextView
                            android:id="@+id/tv_frag_zjd_cz_zlrk"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:text="xx"
                            android:textSize="24sp"
                            android:textStyle="bold"
                            android:textColor="@color/white"
                            android:layout_marginLeft="10dp"
                            android:layout_marginBottom="24.5dp"
                            />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:text="租赁人口（人）"
                            android:textSize="14sp"
                            android:textColor="@color/white"
                            android:layout_marginLeft="10dp"
                            android:layout_marginBottom="6.5dp"
                            />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/wy_zlrs_icon"
                            android:layout_marginTop="22dp"
                            android:layout_marginRight="15dp"
                            android:layout_alignParentRight="true"
                            />

                    </RelativeLayout>
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:background="@color/white"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                >
                <View
                    android:layout_width="5dp"
                    android:layout_height="15dp"
                    android:background="#12C583"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="出租房屋统计"
                    android:textColor="#333333"
                    android:textSize="@dimen/sp_16"
                    android:textStyle="bold"
                    android:layout_marginLeft="10dp"
                    />


            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="#333333"/>

            <com.github.mikephil.charting.charts.BarChart
                android:id="@+id/bct_frag_zjd_cz"
                android:layout_width="match_parent"
                android:layout_height="220dp"/>

            <LinearLayout
                android:background="@color/white"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                >
                <View
                    android:layout_width="5dp"
                    android:layout_height="15dp"
                    android:background="#12C583"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="列表详情"
                    android:textColor="#333333"
                    android:textSize="@dimen/sp_16"
                    android:textStyle="bold"
                    android:layout_marginLeft="10dp"
                    />


            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="#333333"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="35dp"
                android:orientation="horizontal"
                android:gravity="center">
                <TextView
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:paddingLeft="14dp"
                    android:text="村名"
                    android:textColor="#333333"
                    android:textSize="11sp"
                    android:gravity="left"/>
                <TextView
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="出租宅基地(宗)"
                    android:textColor="#333333"
                    android:textSize="11sp"
                    android:gravity="center"/>

                <TextView
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="出租房屋(间)"
                    android:textColor="#333333"
                    android:textSize="11sp"
                    android:gravity="center"/>
                <TextView
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="流动人口"
                    android:textColor="#333333"
                    android:textSize="11sp"
                    android:gravity="center"/>
                <View
                    android:layout_width="30dp"
                    android:layout_height="wrap_content"/>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1.5dp"
                android:background="#666666"/>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">


                <com.jymj.zhglxt.widget.OptimizedRecyclerViewZhen
                    android:id="@+id/rlv_frag_zjd_cz"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                </com.jymj.zhglxt.widget.OptimizedRecyclerViewZhen>

            </RelativeLayout>

            <include
                android:id="@+id/include_frag_zjd_cz"
                layout="@layout/nodata_include"/>
        </LinearLayout>
    </ScrollView>



    
</LinearLayout>
