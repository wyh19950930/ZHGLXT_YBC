<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal" android:layout_width="match_parent"
    android:layout_height="40dp">
    <TextView
        android:id="@+id/item_rjhj_gddw_xh"
        android:text="序号"
        style="@style/table_title_textview"
        android:layout_width="60dp"
        android:layout_height="match_parent"/>
    <TextView
        android:id="@+id/item_rjhj_gddw_xzq"
        android:text="行政区"
        style="@style/table_title_textview"
        android:layout_width="100dp"
        android:layout_height="match_parent"/>
    <TextView
        android:id="@+id/item_rjhj_gddw_wtlx"
        android:text="问题类型"
        style="@style/table_title_textview"
        android:layout_width="100dp"
        android:layout_height="match_parent"/>
    <TextView
        android:id="@+id/item_rjhj_gddw_bh"
        android:text="编号"
        style="@style/table_title_textview"
        android:layout_width="100dp"
        android:layout_height="match_parent"/>
    <TextView
        android:id="@+id/item_rjhj_gddw_wz"
        android:text="位置"
        style="@style/table_title_textview"
        android:layout_width="100dp"
        android:layout_height="match_parent"/>
    <TextView
        android:visibility="gone"
        android:id="@+id/item_rjhj_gddw_lrr"
        android:text="录入人"
        style="@style/table_title_textview"
        android:layout_width="80dp"
        android:layout_height="match_parent"/>
    <TextView
        android:visibility="gone"
        android:id="@+id/item_rjhj_gddw_kfsj"
        android:text="开放时间"
        style="@style/table_title_textview"
        android:layout_width="100dp"
        android:layout_height="match_parent"/>
    <TextView
        android:id="@+id/item_rjhj_gddw_remark"
        android:text="备注"
        style="@style/table_title_textview"
        android:layout_width="150dp"
        android:layout_height="match_parent"/>
    <TextView
        android:visibility="gone"
        android:id="@+id/item_rjhj_gddw_update"
        android:text="修改"
        style="@style/table_title_textview"
        android:textColor="@color/blue"
        android:layout_width="50dp"
        android:layout_height="match_parent"/>
    <TextView
        android:visibility="gone"
        android:id="@+id/item_rjhj_gddw_cz"
        android:text="删除"
        style="@style/table_title_textview"
        android:textColor="@color/red"
        android:layout_width="50dp"
        android:layout_height="match_parent"/>

</LinearLayout>