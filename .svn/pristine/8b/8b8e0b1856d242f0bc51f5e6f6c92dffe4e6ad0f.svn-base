<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:gravity="center"
    android:layout_margin="10dp"
    android:background="@drawable/bt_actiive_xaiyoubai_white">

    <RelativeLayout
        android:background="@drawable/bt_actiive_shangbai_blue2"
        android:layout_gravity="center_horizontal"
        android:layout_width="@dimen/margin_zdkd"
        android:layout_height="40dp">
        <TextView
            android:layout_centerInParent="true"
            android:textSize="16sp"
            android:textColor="@color/white"
            android:text="添加信息"
            android:gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="#cccccc"/>


    <LinearLayout
        android:layout_margin="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:layout_marginBottom="@dimen/margin_sxjg"
            android:layout_marginTop="@dimen/margin_sxjg"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:orientation="horizontal">
<!--            选择年份-->
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="年份"
                android:textColor="#333333"
                android:gravity="center"/>

<!--            android:drawableRight="@drawable/back_down_icon"-->
            <TextView
                android:id="@+id/tv_pop_add_wcwgbdqk"
                android:textColor="#4CA2FE"
                android:hint=""
                android:gravity="right|center_vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"/>
            <Spinner
                android:visibility="gone"
                android:id="@+id/pop_add_wcwgbdqk_spr"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            </Spinner>

        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:background="#cccccc"/>
        <com.jymj.zhglxt.widget.EnableLinearLayout
            android:id="@+id/pop_add_wcwgbdqk_ell"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_weight="3"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:text="长期外出务工人员总数(1年中≥6个月)"
                    android:textColor="#333333"
                    android:gravity="center"/>

                <EditText
                    android:background="@drawable/shape_list_table_title2"
                    android:textColorHint="@color/white"
                    android:hint="请输入长期外出务工人员总数(1年中≥6个月)"
                    android:id="@+id/pop_add_wcwgbdqk_et_scwcwg"
                    android:layout_weight="2"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:inputType="number|numberDecimal"
                    android:maxLength="11"
                    android:singleLine="true"
                    />

            </LinearLayout>
            <LinearLayout
                android:layout_marginBottom="@dimen/margin_sxjg"
                android:layout_marginTop="@dimen/margin_sxjg"
                android:layout_width="match_parent"
                android:layout_height="@dimen/margin_bggd"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_weight="3"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:text="村外乡内"
                    android:textColor="#333333"
                    android:gravity="center_vertical"/>

                <EditText
                    android:background="@drawable/shape_list_table_title2"
                    android:textColorHint="@color/white"
                    android:hint="请输入村外乡内"
                    android:id="@+id/pop_add_wcwgbdqk_et_bdjy"
                    android:layout_weight="2"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:inputType="number|numberDecimal"
                    android:maxLength="11"
                    android:singleLine="true"
                    />

            </LinearLayout>
            <LinearLayout
                android:layout_marginBottom="@dimen/margin_sxjg"
                android:layout_marginTop="@dimen/margin_sxjg"
                android:layout_width="match_parent"
                android:layout_height="@dimen/margin_bggd"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_weight="3"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:text="乡外区内"
                    android:textColor="#333333"
                    android:gravity="center_vertical"/>

                <EditText
                    android:background="@drawable/shape_list_table_title2"
                    android:textColorHint="@color/white"
                    android:hint="请输入乡外区内"
                    android:id="@+id/pop_add_wcwgbdqk_et_xwqn"
                    android:layout_weight="2"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:inputType="number|numberDecimal"
                    android:maxLength="11"
                    android:singleLine="true"
                    />

            </LinearLayout>
            <LinearLayout
                android:layout_marginBottom="@dimen/margin_sxjg"
                android:layout_marginTop="@dimen/margin_sxjg"
                android:layout_width="match_parent"
                android:layout_height="@dimen/margin_bggd"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_weight="3"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:text="区外市内"
                    android:textColor="#333333"
                    android:gravity="center_vertical"/>

                <EditText
                    android:background="@drawable/shape_list_table_title2"
                    android:textColorHint="@color/white"
                    android:hint="请输入区外市内"
                    android:id="@+id/pop_add_wcwgbdqk_et_qwsn"
                    android:layout_weight="2"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:inputType="number|numberDecimal"
                    android:maxLength="11"
                    android:singleLine="true"
                    />

            </LinearLayout>
            <LinearLayout
                android:layout_marginBottom="@dimen/margin_sxjg"
                android:layout_marginTop="@dimen/margin_sxjg"
                android:layout_width="match_parent"
                android:layout_height="@dimen/margin_bggd"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_weight="3"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:text="外省市"
                    android:textColor="#333333"
                    android:gravity="center_vertical"/>

                <EditText
                    android:background="@drawable/shape_list_table_title2"
                    android:textColorHint="@color/white"
                    android:hint="请输入外省市"
                    android:id="@+id/pop_add_wcwgbdqk_et_wss"
                    android:layout_weight="2"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:inputType="number|numberDecimal"
                    android:maxLength="11"
                    android:singleLine="true"
                    />

            </LinearLayout>

        </com.jymj.zhglxt.widget.EnableLinearLayout>
        <LinearLayout
            android:layout_marginBottom="@dimen/margin_sxjg"
            android:layout_marginTop="@dimen/margin_sxjg"
            android:layout_width="match_parent"
            android:layout_height="@dimen/margin_bggd"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:orientation="horizontal">

            <TextView
                android:layout_weight="3"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="操作人姓名"
                android:textColor="#333333"
                android:textStyle="bold"
                android:gravity="center_vertical"/>

            <EditText
                android:background="@drawable/shape_list_table_title2"
                android:id="@+id/pop_add_wcwgbdqk_et_czrname"
                android:layout_weight="2"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:singleLine="true"
                />

        </LinearLayout>
        <LinearLayout
            android:id="@+id/pop_add_wcwgbdqk_ll_reject"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="驳回原因"
                android:textColor="#333333"
                android:textStyle="bold"
                android:gravity="center"/>

            <EditText
                android:id="@+id/pop_add_wcwgbdqk_et_reject"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="right"
                android:singleLine="true"
                />

        </LinearLayout>

    </LinearLayout>


    <View
        android:background="#cccccc"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginTop="5dp"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal">
        <TextView
            android:background="@drawable/bt_actiive_xaizuobai_blue"
            android:visibility="gone"
            android:id="@+id/pop_add_wcwgbdqk_confirm"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:text="确定"
            android:textColor="@color/white"
            android:textSize="16sp"
            android:gravity="center"/>
        <!--<View
            android:layout_width="1dp"
            android:layout_height="18dp"
            android:layout_gravity="center"
            android:background="#E0E0E0"/>-->
        <TextView
            android:background="@drawable/bt_actiive_xaizuobai_blue"
            android:id="@+id/pop_add_wcwgbdqk_reject"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:text="驳回"
            android:textColor="@color/white"
            android:textSize="16sp"
            android:gravity="center"/>
        <View
            android:layout_width="1dp"
            android:layout_height="18dp"
            android:layout_gravity="center"
            android:background="#E0E0E0"/>
        <TextView
            android:id="@+id/pop_add_wcwgbdqk_close"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:text="取消"
            android:textColor="#4CA2FE"
            android:textSize="16sp"
            android:gravity="center"/>
    </LinearLayout>

</LinearLayout>