<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/rl_pop_modify"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:background="@drawable/mapback_ffffff"
        android:orientation="vertical">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:gravity="center"
            android:text="新增车辆"
            android:textColor="#333333"
            android:textSize="16sp"
            android:textStyle="bold" />

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_marginTop="10dp"
            android:background="#e0e0e0" />



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="14.5dp"
            android:layout_marginRight="12.5dp"
            android:gravity="center"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="类型"
                android:textColor="#999999"
                android:textSize="14sp" />

            <RadioGroup
                android:id="@+id/rgp_clxxadd_lx"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="right|center"
                android:orientation="horizontal">

                <RadioButton
                    android:id="@+id/rbt_clxxadd_lx_ddc"
                    android:layout_width="60dp"
                    android:layout_height="25dp"
                    android:background="@drawable/radiobutton_background"
                    android:button="@null"
                    android:checked="true"
                    android:gravity="center"
                    android:text="电动车"
                    android:textColor="@drawable/radiobutton_textcolor"
                    android:textSize="14sp" />

                <RadioButton
                    android:id="@+id/rbt_clxxadd_lx_mtc"
                    android:layout_width="60dp"
                    android:layout_height="25dp"
                    android:layout_marginLeft="10dp"
                    android:background="@drawable/radiobutton_background"
                    android:button="@null"
                    android:gravity="center"
                    android:text="摩托车"
                    android:textColor="@drawable/radiobutton_textcolor"
                    android:textSize="14sp" />

                <RadioButton
                    android:id="@+id/rbt_clxxadd_lx_jdc"
                    android:layout_width="60dp"
                    android:layout_height="25dp"
                    android:layout_marginLeft="10dp"
                    android:background="@drawable/radiobutton_background"
                    android:button="@null"
                    android:gravity="center"
                    android:text="机动车"
                    android:textColor="@drawable/radiobutton_textcolor"
                    android:textSize="14sp" />
            </RadioGroup>
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_marginLeft="14.5dp"
            android:layout_marginRight="12.5dp"
            android:background="#eeeeee" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="14.5dp"
            android:layout_marginRight="12.5dp"
            android:gravity="center"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="品牌"
                android:textColor="#999999"
                android:textSize="14sp" />

            <EditText
                android:id="@+id/edt_clxxadd_pp"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@null"
                android:gravity="right|center"
                android:hint="请输入品牌"
                android:inputType="text"
                android:textColor="#333333"
                android:textSize="14sp"
                android:singleLine="true"/>

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_marginLeft="14.5dp"
            android:layout_marginRight="12.5dp"
            android:background="#eeeeee" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="14.5dp"
            android:layout_marginRight="12.5dp"
            android:gravity="center"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="车牌号"
                android:textColor="#999999"
                android:textSize="14sp" />

            <EditText
                android:id="@+id/edt_clxxadd_cph"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@null"
                android:gravity="right|center"
                android:hint="请输入车牌号"
                android:inputType="text"
                android:textColor="#333333"
                android:textSize="14sp"
                android:singleLine="true"/>

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_marginLeft="14.5dp"
            android:layout_marginRight="12.5dp"
            android:background="#eeeeee" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="14.5dp"
            android:layout_marginRight="12.5dp"
            android:gravity="center"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="车主"
                android:textColor="#999999"
                android:textSize="14sp" />

            <EditText
                android:id="@+id/edt_clxxadd_cz"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@null"
                android:gravity="right|center"
                android:hint="请输入车主"
                android:inputType="text"
                android:textColor="#333333"
                android:textSize="14sp"
                android:singleLine="true"/>

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_marginLeft="14.5dp"
            android:layout_marginRight="12.5dp"
            android:background="#eeeeee" />


        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_marginTop="20dp"
            android:background="#e0e0e0" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/bt_clxxadd_yes"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="保存"
                android:textColor="#4CA2FE"
                android:textSize="16sp" />

            <View
                android:layout_width="0.5dp"
                android:layout_height="18dp"
                android:background="#e0e0e0"
                android:layout_gravity="center" />

            <TextView
                android:id="@+id/bt_clxxadd_no"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="取消"
                android:textColor="#999999"
                android:textSize="16sp" />

        </LinearLayout>


        <!--<android.support.design.widget.TextInputLayout
            android:visibility="gone"
            android:id="@+id/til_username_modify"
            android:layout_width="match_parent"
